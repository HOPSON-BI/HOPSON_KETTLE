    <connection>hs</connection>
    <sql>with dim_subject as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;	DISTINCT&#xd;&#xa;	subject_code,&#xd;&#xa;	subject_name,&#xd;&#xa;	f_code,&#xd;&#xa;	subject_level,&#xd;&#xa;	is_last,&#xd;&#xa;	subject_px,&#xd;&#xa;  ZZ_YuanBianMa,&#xd;&#xa;  fx&#xd;&#xa;from HS.dbo.DIM_PROFIT_YUAN_SUBJECT_MAP a&#xd;&#xa;where format&#x3d;&#x27;4&#x27;&#xd;&#xa;and ndmark&#x3d;year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;--and subject_code like &#x27;DSFZ.01&#x25;&#x27;&#xd;&#xa;--ORDER BY subject_px&#xd;&#xa;&#x29;&#xd;&#xa;,subject as&#x28;&#xd;&#xa;select&#xd;&#xa; &#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.f_code,&#xd;&#xa;a.subject_level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;a.ZZ_YuanBianMa,&#xd;&#xa;a.fx,&#xd;&#xa;CAST&#x28;a.subject_code&#x2b;&#x27;,&#x27; AS VARCHAR&#x28;max&#x29;&#x29; AS fullpath&#xd;&#xa;from dim_subject a&#xd;&#xa;where len&#x28;a.f_code&#x29; &#x3d; 0&#xd;&#xa;--ORDER BY subject_px&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;--DISTINCT &#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.f_code,&#xd;&#xa;a.subject_level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;a.ZZ_YuanBianMa,&#xd;&#xa;a.fx,&#xd;&#xa;cast&#x28;subject.fullpath &#x2b; &#x27;-&#x3e;&#x27;&#x2b; a.subject_code&#x2b;&#x27;,&#x27;  as VARCHAR&#x28;max&#x29;&#x29;as fullpath&#xd;&#xa;from dim_subject a&#xd;&#xa;inner join subject  on subject.subject_code &#x3d; a.f_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from subject --WHERE subject_code&#x3d;&#x27;DSFZ.01.04&#x27; &#xd;&#xa;--ORDER BY subject_px&#xd;&#xa;,month_data as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;	project_code,&#xd;&#xa;	subject_code,&#xd;&#xa;	subject_name,&#xd;&#xa;	&#x5b;year&#x5d;,&#xd;&#xa;	12 as &#x5b;month&#x5d;,&#xd;&#xa;	sum&#x28;isnull&#x28;&#x5b;value&#x5d;,0&#x29;&#x29; as value&#xd;&#xa; from YW_YY_PROFIT_ANALYST_RESULT&#xd;&#xa;where project_code IN &#x28;SELECT DISTINCT project_code FROM YW_HS_PROJECT_BASEINFO_ZZ_NEW&#xd;&#xa;where show&#x3d;1&#xd;&#xa;and business in &#x28;&#x27;&#x5546;&#x573a;&#x27;,&#x27;&#x5199;&#x5b57;&#x697c;&#x27;,&#x27;&#x4ea7;&#x4e1a;&#x56ed;&#x27;,&#x27;&#x7efc;&#x5408;&#x4f53;&#x27;&#x29;&#x29;&#xd;&#xa; AND  year&#x3c;year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;--and  subject_code in  &#x28;&#x27;SR1010107010701020&#x27;&#x29;&#xd;&#xa;GROUP BY &#xd;&#xa;project_code,&#xd;&#xa;	subject_code,&#xd;&#xa;	subject_name,&#xd;&#xa;	&#x5b;year&#x5d;&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;--SELECT &#x2a; from pro_sub ORDER BY subject_px&#xd;&#xa;,moji_sj as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;b.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.f_code,&#xd;&#xa;a.subject_level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;b.year,&#xd;&#xa;b.month,&#xd;&#xa;a.fullpath,&#xd;&#xa;sum&#x28;isnull&#x28;b.value,0&#x29;&#x2a;a.fx&#x29; as ls_sj&#xd;&#xa;from  &#xd;&#xa;subject  a&#xd;&#xa;inner JOIN month_data b&#xd;&#xa;on b.subject_code&#x3d;a.ZZ_YuanBianMa&#xd;&#xa;--and a.project_code&#x3d;b.project_code&#xd;&#xa;--and a.year&#x3d;b.year&#xd;&#xa;--and a.month&#x3d;b.month&#xd;&#xa;WHERE a.is_last&#x3d;1&#xd;&#xa;group by &#xd;&#xa;b.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.f_code,&#xd;&#xa;a.subject_level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;b.year,&#xd;&#xa;b.month,&#xd;&#xa;a.fullpath&#xd;&#xa;--ORDER BY year,month,subject_px&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from moji_sj&#xd;&#xa;,erji_data as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;b.subject_code,&#xd;&#xa;b.subject_name,&#xd;&#xa;b.subject_level,&#xd;&#xa;b.subject_px,&#xd;&#xa;b.is_last,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;b.fullpath,&#xd;&#xa;--a.fullpath as fullpaths,&#xd;&#xa;SUM&#x28;a.ls_sj&#x29; as ls_sj&#xd;&#xa;from  subject b &#xd;&#xa;left JOIN moji_sj  a&#xd;&#xa;on charindex&#x28;b.fullpath,a.fullpath&#x29;&#x3d;1&#xd;&#xa;WHERE b.is_last&#x3d;0&#xd;&#xa;GROUP BY &#xd;&#xa;a.project_code,&#xd;&#xa;b.subject_code,&#xd;&#xa;b.subject_name,&#xd;&#xa;b.subject_level,&#xd;&#xa;b.subject_px,&#xd;&#xa;b.is_last,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;b.fullpath&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;b.project_code,&#xd;&#xa;b.subject_code,&#xd;&#xa;b.subject_name,&#xd;&#xa;b.subject_level,&#xd;&#xa;b.subject_px,&#xd;&#xa;b.is_last,&#xd;&#xa;b.year,&#xd;&#xa;b.month,&#xd;&#xa;b.fullpath,&#xd;&#xa;b.ls_sj&#xd;&#xa;FROM moji_sj b&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from  erji_data&#xd;&#xa;,yiji_data as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;b.subject_code,&#xd;&#xa;b.subject_name,&#xd;&#xa;b.subject_level,&#xd;&#xa;b.subject_px,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;b.is_last,&#xd;&#xa;b.fullpath,&#xd;&#xa;--a.fullpath as fullpaths,&#xd;&#xa;sum&#x28;a.ls_sj&#x29; as ls_sj&#xd;&#xa;from  subject b &#xd;&#xa;left JOIN erji_data  a&#xd;&#xa;on charindex&#x28;b.fullpath,a.fullpath&#x29;&#x3d;1&#xd;&#xa;WHERE b.is_last&#x3d;2&#xd;&#xa;and a.subject_level&#x3d;2&#xd;&#xa;GROUP BY &#xd;&#xa;a.project_code,&#xd;&#xa;b.subject_code,&#xd;&#xa;b.subject_name,&#xd;&#xa;b.subject_level,&#xd;&#xa;b.subject_px,&#xd;&#xa;b.is_last,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;b.fullpath&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;b.project_code,&#xd;&#xa;b.subject_code,&#xd;&#xa;b.subject_name,&#xd;&#xa;b.subject_level,&#xd;&#xa;b.subject_px,&#xd;&#xa;b.year,&#xd;&#xa;b.month,&#xd;&#xa;b.is_last,&#xd;&#xa;b.fullpath,&#xd;&#xa;b.ls_sj&#xd;&#xa;FROM erji_data b&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from yiji_data --where subject_code in &#x28;&#x27;DSYZ2&#x27;,&#x27;DSYZ2.1&#x27;,&#x27;DSYZ2.2&#x27;,&#x27;DSYZ2.3&#x27;,&#x27;DSYZ2.4&#x27;,&#x27;DSYZ2.5&#x27;,&#x27;DSYZ2.6&#x27;,&#x27;DSYZ2.7&#x27;,&#x27;DSYZ2.8&#x27;&#xd;&#xa;--,&#x27;DSYZ2.9&#x27;,&#x27;DSYZ2.10&#x27;,&#x27;DSYZ2.11&#x27;,&#x27;DSYZ2.12&#x27;,&#x27;DSYZ2.13&#x27;,&#x27;DSYZ2.14&#x27;,&#x27;DSYZ2.15&#x27;,&#x27;DSYZ2.16&#x27;,&#x27;DSYZ2.17&#x27;,&#x27;DSYZ2.19&#x27;,&#x27;DSYZ2.18&#x27;,&#x27;DSYZ2.20&#x27;,&#x27;DSYZ2.21&#x27;&#x29; &#xd;&#xa;-- and year&#x3d;&#x27;2018&#x27;&#xd;&#xa;--ORDER BY subject_px&#xd;&#xa;,last_data as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;b.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.f_code,&#xd;&#xa;a.subject_level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;b.year,&#xd;&#xa;b.month,&#xd;&#xa;--a.fullpath,&#xd;&#xa;sum&#x28;isnull&#x28;b.ls_sj,0&#x29;&#x2a;a.fx&#x29; as ls_sj&#xd;&#xa;from  &#xd;&#xa;subject  a&#xd;&#xa;inner JOIN yiji_data b&#xd;&#xa;on b.subject_code&#x3d;a.ZZ_YuanBianMa&#xd;&#xa;where a.is_last&#x3d;3&#xd;&#xa;group by &#xd;&#xa;b.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.f_code,&#xd;&#xa;a.subject_level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;b.year,&#xd;&#xa;b.month&#xd;&#xa;--a.fullpath&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name,&#xd;&#xa;--f_code,&#xd;&#xa;subject_level,&#xd;&#xa;is_last,&#xd;&#xa;subject_px,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;--a.fullpath,&#xd;&#xa;ls_sj&#xd;&#xa;--year&#x28;getdate&#x28;&#x29;&#x29; as mark&#xd;&#xa;from yiji_data&#xd;&#xa;&#x29;&#xd;&#xa;SELECT &#xd;&#xa; a.&#x2a;,&#xd;&#xa;year&#x28;getdate&#x28;&#x29;&#x29; as mark&#xd;&#xa;from last_data a &#xd;&#xa;--wherE project_code&#x3d;&#x27;BJ_HSH_SDDJZHT&#x27;&#xd;&#xa;--AND SUBJECT_CODE IN &#x28;&#x27;DSSR1&#x27;,&#x27;DSYZ2&#x27;,&#x27;DSDANDA3&#x27;&#x29;&#xd;&#xa;--AND YEAR&#x3d;&#x27;2018&#x27;&#xd;&#xa;ORDER BY year,month,subject_px</sql>
