x27;GZ&#x27;,&#x27;FY&#x27;&#x29;&#xd;&#xa;and len&#x28;subject_code&#x29;&#x3d;6&#xd;&#xa;group by project_code,YEAR,month&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;--&#x9152;&#x5e97;&#x6536;&#x652f;&#x6570;&#x636e;&#xd;&#xa;jdsz AS&#x28;&#xd;&#xa;select &#xd;&#xa;		isnull&#x28;sr.project_code,zc.project_code&#x29; as project_code,&#xd;&#xa;		&#x27;SZ101010&#x27; AS subject_code,&#xd;&#xa;		&#x27;&#x6536;&#x5165;&#x652f;&#x51fa;&#x27; AS subject_name,&#xd;&#xa;		isnull&#x28;sr.year,zc.year&#x29; as year,&#xd;&#xa;		isnull&#x28;sr.month,zc.month&#x29; as month,&#xd;&#xa;		case when isnull&#x28;sr.actual,0&#x29;-isnull&#x28;zc.actual,0&#x29;&#x3c;0 then 0&#xd;&#xa;		else isnull&#x28;sr.actual,0&#x29;-isnull&#x28;zc.actual,0&#x29; end as actual_gop&#xd;&#xa;from sr full join zc on sr.project_code&#x3d;zc.project_code and sr.year&#x3d;zc.year and sr.month&#x3d;zc.month&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;--&#x9884;&#x7b97;&#x8fbe;&#x6210;&#x6570;&#x636e;&#xd;&#xa;ysdcsjlr AS&#x28;&#xd;&#xa;select &#xd;&#xa;      a.project_code,&#xd;&#xa;			 &#x27;LR101010&#x27; AS subject_code,&#xd;&#xa;      replace&#x28;a.classify,&#x27;&#x51c0;&#x5229;&#x6da6;&#x27;,&#x27;EBITDA&#x27;&#x29; as subject_name,&#xd;&#xa;			b.year,b.month,&#xd;&#xa;      isnull&#x28;b.tax_amount,0&#x29; as tax_amount&#xd;&#xa;from orderkey a &#xd;&#xa;left join sle b on a.project_code &#x3d; b.project_code and a.classify &#x3d; b.classify &#xd;&#xa;WHERE a.classify&#x3c;&#x3e;&#x27;&#x603b;&#x6536;&#x5165;&#x27;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;--&#x9884;&#x7b97;&#x8fbe;&#x6210;&#x6570;&#x636e;&#xd;&#xa;ysdcsjsr AS&#x28;&#xd;&#xa;select &#xd;&#xa;      a.project_code,&#xd;&#xa;			&#x27;ZS101010&#x27; AS subject_code,&#xd;&#xa;      a.classify as subject_name,&#xd;&#xa;			b.year,b.month,&#xd;&#xa;      isnull&#x28;b.tax_amount,0&#x29; as tax_amount&#xd;&#xa;from orderkey a &#xd;&#xa;left join sle b on a.project_code &#x3d; b.project_code and a.classify &#x3d; b.classify &#xd;&#xa;WHERE a.classify&#x3d;&#x27;&#x603b;&#x6536;&#x5165;&#x27;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;--&#x7ecf;&#x8425;&#x8d44;&#x4ea7;&#x644a;&#x9500;&#xd;&#xa;jyzctx AS&#x28;&#xd;&#xa;select &#xd;&#xa;      project_code as project_code,&#xd;&#xa;      cast&#x28;subject_code as VARCHAR&#x28;10&#x29;&#x29; as subject_code,&#xd;&#xa;      subject_name as subject_name,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;      sum&#x28;isnull&#x28;actualnotax,0&#x29;&#x29; as actualnotax  &#xd;&#xa;FROM YW_YY_BENEFIT_ANALYST&#xd;&#xa;where subject_code&#x3d;&#x27;1106&#x27;&#xd;&#xa;group by project_code,sequence,subject_code,subject_name,year,month&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;VALUE_s AS&#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.business,&#xd;&#xa;a.project_code,&#xd;&#xa;b.year,&#xd;&#xa;b.month,&#xd;&#xa;b.tax_amount AS lr,&#xd;&#xa;c.tax_amount AS sr,&#xd;&#xa;d.actual_gop AS jdsz,&#xd;&#xa;e.actualnotax AS tx&#xd;&#xa;FROM XMJB a&#xd;&#xa;LEFT JOIN &#xd;&#xa;ysdcsjlr b&#xd;&#xa;ON a.project_code&#x3d;b.project_code&#xd;&#xa;LEFT JOIN &#xd;&#xa;ysdcsjsr c&#xd;&#xa;ON a.project_code&#x3d;c.project_code&#xd;&#xa;AND b.year&#x3d;c.year&#xd;&#xa;AND b.month&#x3d;c.month&#xd;&#xa;LEFT JOIN &#xd;&#xa;jdsz d&#xd;&#xa;ON a.project_code&#x3d;d.project_code&#xd;&#xa;AND c.year&#x3d;d.year&#xd;&#xa;AND c.month&#x3d;d.month&#xd;&#xa;LEFT JOIN&#xd;&#xa;jyzctx e&#xd;&#xa;ON a.project_code&#x3d;e.project_code&#xd;&#xa;AND c.year&#x3d;e.year&#xd;&#xa;AND c.month&#x3d;e.month&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;--EBITDA2&#xd;&#xa;EBITDA2 AS &#x28;&#xd;&#xa;SELECT&#xd;&#xa;				project_code,&#xd;&#xa;				year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AS year,&#xd;&#xa;				month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AS month,&#xd;&#xa;				&#x27;&#x7ecf;&#x8425;&#x8d26;&#x27; AS category,&#xd;&#xa;				&#x27;1010&#x27; AS category_code,&#xd;&#xa;				&#x27;LR101010&#x27; AS subject_code,&#xd;&#xa;				&#x27;EBITDA2&#x27; AS subject_name,&#xd;&#xa;sum&#x28;CASE WHEN business&#x3d;&#x27;&#x9152;&#x5e97;&#x27; THEN&#xd;&#xa;		CASE WHEN project_code in &#x28;&#x27;SZ_JD_HGJR&#x27;,&#x27;SH_FJY_KYJXJD&#x27;,&#x27;SH_JD_WJCKY&#x27;,&#x27;FS_JD_KYJX&#x27;,&#x27;TJ_JD_BDKY&#x27;&#x29; THEN ISNULL&#x28;lr, 0&#x29;-&#x28;isnull&#x28;jdsz,0&#x29;&#x2a;0.02&#x29;--&#x59d4;&#x5916;&#x9152;&#x5e97;&#xd;&#xa;					ELSE ISNULL&#x28;lr, 0&#x29;-&#x28;&#x28;isnull&#x28;sr,0&#x29;&#x2b;isnull&#x28;jdsz,0&#x29;&#x29;&#x2a;0.02&#x29;&#xd;&#xa;&#xd;&#xa;		END&#xd;&#xa;ELSE&#xd;&#xa;		CASE WHEN ISNULL&#x28;lr,0&#x29;&#x3c;&#x3d;0 THEN ISNULL&#x28;lr,0&#x29;-ISNULL&#x28;tx, 0&#x29;-&#x28;isnull&#x28;sr,0&#x29;&#x2a;0.02&#x29; ELSE ISNULL&#x28;lr,0&#x29;-ISNULL&#x28;tx, 0&#x29;-&#x28;&#x28;isnull&#x28;sr,0&#x29;&#x2b;ISNULL&#x28;lr,0&#x29;&#x29;&#x2a;0.02&#x29; END&#xd;&#xa;END&#x29;&#x2a;10000 AS value,&#xd;&#xa;					getdate&#x28;&#x29; as timestamp&#xd;&#xa;FROM VALUE_s&#xd;&#xa;WHERE year&#x3d;year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AND month&#x3c;&#x3d;month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; OR year&#x3c; year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29;&#xd;&#xa;GROUP BY &#xd;&#xa;				project_code&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;EBITDA AS&#x28;&#xd;&#xa;SELECT a.project_code ,&#xd;&#xa;			CASE WHEN b.business&#x3d;&#x27;&#x9152;&#x5e97;&#x27; THEN &#x27;2&#x27; &#xd;&#xa; 				WHEN b.business&#x3d;&#x27;&#x9910;&#x996e;&#x27;&#xd;&#xa;					THEN &#x27;3&#x27; ELSE &#x27;1&#x27; END  AS business ,&#xd;&#xa;					b.&#x5b;group&#x5d;,&#xd;&#xa;					&#x27;LR101010&#x27; AS subject_code,&#xd;&#xa;					&#x27;EBITDA2&#x27; AS subject_name,&#xd;&#xa;					year,&#xd;&#xa;					month,&#xd;&#xa;					value AS money&#xd;&#xa;FROM EBITDA2 a&#xd;&#xa;LEFT JOIN XMJB b&#xd;&#xa;ON a.project_code&#x3d;b.project_code&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;sje AS &#x28;&#xd;&#xa;SELECT &#x2a; FROM sj&#xd;&#xa;UNION ALL &#xd;&#xa;SELECT &#x2a; FROM EBITDA&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;Last_value AS&#x28;&#xd;&#xa;select &#xd;&#xa;				b.project_code,&#xd;&#xa;				b.&#x5b;year&#x5d;,&#xd;&#xa;				b.&#x5b;month&#x5d;,&#xd;&#xa;				a.category,&#xd;&#xa;				a.category_code,&#xd;&#xa;				a.BI_subject_code as subject_code,&#xd;&#xa;				a.BI_subject_name as subject_name,&#xd;&#xa;				sum&#x28;a.proportion&#x2a;b.money&#x29; as &#x5b;value&#x5d;,&#xd;&#xa;				getdate&#x28;&#x29; as timestamp&#xd;&#xa;	&#xd;&#xa;from  sje b&#xd;&#xa;left join YW_HS_DIM_FINANCE_SUM_MAP a on a.subject_code&#x3d;b.subject_code and a.format&#x3d;b.business&#xd;&#xa;where len&#x28;a.BI_subject_code&#x29;&#x3c;&#x3e;0&#xd;&#xa;group by b.project_code,b.&#x5b;year&#x5d;,b.&#x5b;month&#x5d;,a.category,a.category_code,a.BI_subject_code,a.BI_subject_name&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;data_1 AS&#x28;&#xd;&#xa;SELECT &#xd;&#xa;				project_code,&#xd;&#xa;SUM&#x28;value&#x29; AS value_1  FROM Last_value WHERE subject_code IN&#x28;&#x27;LR101060&#x27;,&#x27;ZC103030&#x27;&#x29;&#xd;&#xa;GROUP BY&#xd;&#xa;				project_code&#xd;&#xa;&#x29;,--&#x51c0;&#x5229;&#x6da6;&#x2b;&#x65b0;&#x589e;&#x878d;&#x8d44;&#x989d;&#xd;&#xa;&#xd;&#xa;data_2 AS&#x28;&#xd;&#xa;SELECT &#xd;&#xa;				project_code,&#xd;&#xa;value AS value_2  FROM Last_value WHERE subject_code &#x3d;&#x27;FZ102010&#x27;&#xd;&#xa;&#x29;,--&#x94f6;&#x884c;&#x8d37;&#x6b3e; &#xd;&#xa;&#xd;&#xa;data_3 AS&#x28;&#xd;&#xa;SELECT &#xd;&#xa;				project_code,&#xd;&#xa;value AS value_3  FROM Last_value WHERE subject_code &#x3d;&#x27;ZC102020&#x27;&#xd;&#xa;&#x29;,--&#x603b;&#x6295;&#x5165;&#xd;&#xa;&#xd;&#xa;Confluence AS &#x28;&#xd;&#xa;SELECT data_2.project_code AS project_code_2 , data_3.project_code AS project_code_3 ,data_2.value_2,data_3.value_3   FROM data_2&#xd;&#xa;FULL JOIN data_3 ON	data_2.project_code&#x3d;data_3.project_code&#xd;&#xa;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;--&#x540e;&#x7eed;&#xd;&#xa;hx AS&#x28;&#xd;&#xa;SELECT &#xd;&#xa;				data_1.project_code,&#xd;&#xa;				year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AS year,&#xd;&#xa;				month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AS month,&#xd;&#xa;				&#x27;&#x8d44;&#x672c;&#x8d26;&#x27; AS category,&#xd;&#xa;				&#x27;1030&#x27; AS category_code,&#xd;&#xa;				&#x27;ZC103040&#x27; AS subject_code,&#xd;&#xa;				&#x27;&#x540e;&#x7eed;&#x53d1;&#x5c55;&#x8d44;&#x91d1;&#x603b;&#x989d;&#x27; AS subject_name,&#xd;&#xa;				CASE WHEN ISNULL&#x28; value_2, 0&#x29;-ISNULL&#x28; value_3, 0&#x29;&#x3c;0 THEN value_1 ELSE isnull&#x28;value_1,0&#x29;&#x2b;ISNULL&#x28; value_2, 0&#x29;-ISNULL&#x28; value_3, 0&#x29; END AS &#x5b;value&#x5d;,&#xd;&#xa;				getdate&#x28;&#x29; as timestamp&#xd;&#xa;FROM data_1&#xd;&#xa;LEFT JOIN Confluence ON data_1.project_code&#x3d;ISNULL&#x28;Confluence.project_code_2, Confluence.project_code_3&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT &#x2a;  FROM Last_value&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#x2a; FROM hx&#xd;&#xa;--UNION ALL&#xd;&#xa;&#xd;&#xa;</sql>
