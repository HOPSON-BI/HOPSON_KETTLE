    <sql>WITH  XMJB AS&#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;business,&#xd;&#xa;&#x5b;group&#x5d;,&#xd;&#xa;ISNULL&#x28;orderkey, 509&#x29; AS orderkey&#xd;&#xa;from &#xd;&#xa;YW_HS_PROJECT_BASEINFO_ZZ &#xd;&#xa;WHERE business IN &#x28;&#x27;&#x9152;&#x5e97;&#x27;,&#x27;&#x9910;&#x996e;&#x27;&#x29;&#xd;&#xa;and  len&#x28;&#x5b;group&#x5d;&#x29;&#x3c;&#x3e;0&#xd;&#xa;and len&#x28;business&#x29;&#x3c;&#x3e;0 &#xd;&#xa;UNION ALL&#xd;&#xa;SELECT&#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;business,&#xd;&#xa;&#x5b;group&#x5d;,&#xd;&#xa;orderkey&#xd;&#xa;from &#xd;&#xa;YW_HS_PROJECT_BASEINFO&#xd;&#xa;WHERE business IN &#x28;&#x27;&#x5546;&#x573a;&#x27;,&#x27;&#x5199;&#x5b57;&#x697c;&#x27;,&#x27;&#x4ea7;&#x4e1a;&#x56ed;&#x27;&#x29;&#xd;&#xa;and  len&#x28;&#x5b;group&#x5d;&#x29;&#x3c;&#x3e;0&#xd;&#xa;and len&#x28;business&#x29;&#x3c;&#x3e;0 &#xd;&#xa;&#x29;,--&#x9879;&#x76ee;&#x57fa;&#x672c;&#x4fe1;&#x606f;&#xd;&#xa;&#xd;&#xa;--&#x5229;&#x6da6;&#x660e;&#x7ec6;&#xd;&#xa;PROFIT_ANALYST AS&#x28;&#xd;&#xa;SELECT &#x2a; FROM YW_YY_PROFIT_ANALYST WHERE year&#x3d;year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AND month&#x3c;&#x3d;month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; OR year &#x3c; year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;--&#x8d44;&#x4ea7;&#x660e;&#x7ec6;&#xd;&#xa;ASSETS_ANALYST AS&#x28;&#xd;&#xa;SELECT &#x2a; FROM YW_YY_ASSETS_ANALYST WHERE year&#x3d;year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AND month&#x3d;month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;--&#x8d44;&#x91d1;&#x660e;&#x7ec6;&#xd;&#xa;CAPITAL_ANALYST AS&#x28;&#xd;&#xa;SELECT &#x2a; FROM YW_YY_CAPITAL_ANALYST WHERE year&#x28;yearmonth&#x2b;&#x27;-01&#x27;&#x29;&#x3d;year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AND month&#x28;yearmonth&#x2b;&#x27;-01&#x27;&#x29;&#x3c;&#x3d;month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; OR year&#x28;yearmonth&#x2b;&#x27;-01&#x27;&#x29; &#x3c; year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29;&#xd;&#xa;AND subject_code IN&#x28;&#x27;LR1010&#x27;,&#x27;ZC1050&#x27;,&#x27;ZC1060&#x27;,&#x27;ZC1070&#x27;,&#x27;ZC1080&#x27;,&#x27;YE1090&#x27;,&#x27;ZC1120&#x27;&#x29;--&#x7d2f;&#x8ba1;&#x6570;&#x636e;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#x2a; FROM YW_YY_CAPITAL_ANALYST WHERE year&#x28;yearmonth&#x2b;&#x27;-01&#x27;&#x29;&#x3d;year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AND month&#x28;yearmonth&#x2b;&#x27;-01&#x27;&#x29;&#x3d;month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29;&#xd;&#xa;AND subject_code IN&#x28;&#x27;JE1040&#x27;,&#x27;RZ1100&#x27;,&#x27;ZC1110&#x27;,&#x27;JE1150&#x27;,&#x27;YE1160&#x27;&#x29;--&#x65f6;&#x70b9;&#x6570;&#xd;&#xa;&#xd;&#xa;&#x29;,&#xd;&#xa;--&#x878d;&#x8d44;&#xd;&#xa;FINANCING_ANALYST AS&#x28;&#xd;&#xa;SELECT &#x2a; FROM YW_YY_FINANCING_ANALYST WHERE year&#x3d;year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AND month&#x3c;&#x3d;month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; OR year &#x3c; year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;sj as &#x28;&#xd;&#xa;select &#xd;&#xa;				a.project_code,&#xd;&#xa;				&#x27;1&#x27; as business,&#xd;&#xa;				b.&#x5b;group&#x5d;,&#xd;&#xa;				a.subject_code,&#xd;&#xa;				a.subject_name,&#xd;&#xa;				year&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AS year,&#xd;&#xa;				month&#x28;&#x27;&#x24;&#x7b;yearmonth&#x7d;&#x27;&#x2b;&#x27;-01&#x27;&#x29; AS month,&#xd;&#xa;				sum&#x28;a.&#x5b;value&#x5d;&#x29; as money&#xd;&#xa;from PROFIT_ANALYST a&#xd;&#xa;left join XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;where  a.s