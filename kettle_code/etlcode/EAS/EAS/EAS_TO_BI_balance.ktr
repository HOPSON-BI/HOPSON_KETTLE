    <sql>select&#xd;&#xa;    rawtohex&#x28;sys_guid&#x28;&#x29;&#x29; as FID,&#xd;&#xa;	 t4.fnumber  AS FOrgUnitNum, 				--&#x516c;&#x53f8;&#x7f16;&#x7801;&#xd;&#xa;    T4.FNAME_L2 AS FOrgUnitName, 	--&#x516c;&#x53f8;&#x540d;&#x79f0;&#xd;&#xa;	 T4.FSIMPLENAME AS  FSimpleName, 	--&#x516c;&#x53f8;&#x7b80;&#x79f0;&#xd;&#xa;	 t3.fnumber AS  FAccountNum,		--&#x79d1;&#x76ee;&#x7f16;&#x7801;&#xd;&#xa;	 t3.FNAME_L2 AS FAccountName, 	--&#x79d1;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;	 null AS  FAssistProjectType,--&#x6838;&#x7b97;&#x9879;&#x76ee;&#x7c7b;&#x578b;&#xd;&#xa;	 --null AS FAssistProjectID,	-- &#x6838;&#x7b97;&#x9879;&#x76ee;ID&#xd;&#xa;	 null AS FAssistProjectNum, 	-- &#x6838;&#x7b97;&#x9879;&#x76ee;&#x7f16;&#x7801;&#xd;&#xa;    null AS FAssistProjectName, 	--&#x6838;&#x7b97;&#x9879;&#x76ee;&#x540d;&#x79f0;&#xd;&#xa;	 null AS  IsInsideSupplier, 	--&#x662f;&#x5426;&#x5185;&#x90e8;&#x4f9b;&#x5e94;&#x5546;&#xd;&#xa;    t5.fnumber AS FPeriodNumber,--&#x671f;&#x95f4;&#xd;&#xa;    sum&#x28;T1.FYearDebitLocal&#x29; AS FYearDebitLocal,	--&#x672c;&#x5e74;&#x7d2f;&#x8ba1;&#x501f;&#x65b9;&#x672c;&#x4f4d;&#x5e01;&#xd;&#xa;    sum&#x28;T1.FYearCreditLocal&#x29; AS FYearCreditLocal,	--&#x672c;&#x5e74;&#x7d2f;&#x8ba1;&#x8d37;&#x65b9;&#x672c;&#x4f4d;&#x5e01;&#xd;&#xa;	 sum&#x28;T1.FDebitLocal&#x29; AS FDebitLocal,	--&#x672c;&#x671f;&#x501f;&#x65b9;&#x672c;&#x4f4d;&#x5e01;&#xd;&#xa;	 sum&#x28;T1.FCreditLocal&#x29; AS FCreditLocal, --&#x672c;&#x671f;&#x8d37;&#x65b9;&#x672c;&#x4f4d;&#x5e01;&#xd;&#xa;	 sum&#x28;T1.FMonthPnLLocal&#x2a;t3.FDC&#x29; AS   FMonthPnLLocal,--&#x672c;&#x671f;&#x635f;&#x76ca;&#x53d1;&#x751f;&#x989d;&#x672c;&#x4f4d;&#x5e01;&#xd;&#xa;    case when t3. FDC&#x3d;1 then sum&#x28;T1.FBeginBalanceLocal&#x2a;t3. FDC&#x29; else null end	AS FBeginBalanceDebitLocal,--&#x501f;&#x65b9;&#x521d;&#x59cb;&#x4f59;&#x989d;&#x672c;&#x4f4d;&#x5e01;&#xd;&#xa;    case when t3. FDC&#x3d;-1 then sum&#x28;T1.FBeginBalanceLocal&#x2a;t3. FDC&#x29; else null end	AS FBeginBalanceCreditLocal,--&