    <connection>BI</connection>
    <sql>WITH orderkey AS &#x28;&#xd;&#xa;SELECT DISTINCT project_code,&#xd;&#xa;       project_name,&#xd;&#xa;       orderkey&#xd;&#xa;FROM YW_HS_PROJECT_BASEINFO &#xd;&#xa;WHERE branch &#x3d; &#x27;&#x5546;&#x4e1a;&#x4e8b;&#x4e1a;&#x90e8;&#x27;and project_code not in &#x28;&#x27;GZ_HSH_JH_NQ&#x27;,&#x27;GZ_HSH_JH_BQ&#x27;&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;sale_mx as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;      t1.project_code,&#xd;&#xa;      t1.floor,&#xd;&#xa;      t1.berthid,&#xd;&#xa;      t1.ShopID,&#xd;&#xa;      t1.BizDate,&#xd;&#xa;			t1.PaidAmt,&#xd;&#xa;			t2.ShopName,&#xd;&#xa;			t2.TradeName,&#xd;&#xa;			t2.UserArea&#xd;&#xa;FROM orderkey t3&#xd;&#xa;LEFT JOIN DW_DLDF_ShopSku t1 &#xd;&#xa;on t3.project_code &#x3d; t1.project_code&#xd;&#xa;LEFT JOIN DW_DLDF_ShopBase t2 &#xd;&#xa;on t1.project_code &#x3d; t2.project_code&#xd;&#xa;AND t1.floor &#x3d; t2.floor&#xd;&#xa;AND t1.berthid &#x3d; t2.berthid&#xd;&#xa;AND t1.ShopID &#x3d; t2.ShopID&#xd;&#xa;AND convert&#x28;varchar&#x28;10&#x29;,t2.bizdate,120&#x29; &#x3d; convert&#x28;varchar&#x28;10&#x29;,dateadd&#x28;day,-1,getdate&#x28;&#x29;&#x29;,120&#x29;&#xd;&#xa;--AND t1.BizDate &#x3d; t2.BizDate&#xd;&#xa;WHERE 1 &#x3d; 1 --t2.ContractStatus &#x3d; 2&#xd;&#xa;AND CONVERT&#x28;VARCHAR&#x28;7&#x29;,t1.BizDate,120&#x29; &#x3d; CONVERT&#x28;VARCHAR&#x28;7&#x29;,DATEADD&#x28;month,-1,GETDATE&#x28;&#x29;&#x29;,120&#x29;&#xd;&#xa;--AND t2.ContractStatus &#x3d; 2&#xd;&#xa;--AND t1.project_code &#x3d; &#x27;BJ_HSH_SDDJ&#x27; &#xd;&#xa;--AND t1.ShopID &#x3d; &#x27;5435&#x27;--AND t1.berthid &#x3d; &#x27;L2-46&#x27;&#xd;&#xa;--ORDER BY BizDate&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;      project_code,&#xd;&#xa;      floor,&#xd;&#xa;      berthid,&#xd;&#xa;      ShopID,&#xd;&#xa;      CONVERT&#x28;VARCHAR&#x28;7&#x29;,BizDate,120&#x29; yearmonth,&#xd;&#xa;      sum&#x28;PaidAmt&#x29; PaidAmt,&#xd;&#xa;			max&#x28;UserArea&#x29; UserArea,&#xd;&#xa;			case when max&#x28;UserArea&#x29; &#x3d; 0 then null&#xd;&#xa;			     when max&#x28;UserArea&#x29; is null then null &#xd;&#xa;					 when sum&#x28;PaidAmt&#x29; is null then null&#xd;&#xa;					 when sum&#x28;PaidAmt&#x29; &#x3d; 0 then null&#xd;&#xa;			     else sum&#x28;PaidAmt&#x29;&#x2f;max&#x28;UserArea&#x29; end Level_ground_effect,&#xd;&#xa;			ShopName,&#xd;&#xa;			TradeName,&#xd;&#xa;            GETDATE&#x28;&#x29; as etl_time&#xd;&#xa;FROM sale_mx &#xd;&#xa;GROUP BY project_code,&#xd;&#xa;         floor,&#xd;&#xa;         berthid,&#xd;&#xa;         ShopID,&#xd;&#xa;         CONVERT&#x28;VARCHAR&#x28;7&#x29;,BizDate,120&#x29;,&#xd;&#xa;				 ShopName,&#xd;&#xa;			   TradeName&#xd;&#xa;ORDER BY Level_ground_effect</sql>
