    <connection>BI</connection>
    <sql>WITH orderkey AS &#x28;&#xd;&#xa;SELECT DISTINCT project_code,&#xd;&#xa;       project_name,&#xd;&#xa;       orderkey,&#xd;&#xa;			 CONVERT&#x28;VARCHAR&#x28;4&#x29;,DATEADD&#x28;DAY,-1,GETDATE&#x28;&#x29;&#x29;,120&#x29; as the_year&#xd;&#xa;FROM YW_HS_PROJECT_BASEINFO &#xd;&#xa;WHERE branch &#x3d; &#x27;&#x5546;&#x4e1a;&#x4e8b;&#x4e1a;&#x90e8;&#x27;and project_code not in &#x28;&#x27;GZ_HSH_JH_NQ&#x27;,&#x27;GZ_HSH_JH_BQ&#x27;&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;rq as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;      the_date,&#xd;&#xa;			the_year&#xd;&#xa;FROM Dim_time &#xd;&#xa;WHERE the_year &#x3d; CONVERT&#x28;VARCHAR&#x28;4&#x29;,DATEADD&#x28;DAY,-1,GETDATE&#x28;&#x29;&#x29;,120&#x29;&#xd;&#xa;AND the_date &#x3c;&#x3d; CONVERT&#x28;VARCHAR&#x28;10&#x29;,DATEADD&#x28;DAY,-1,GETDATE&#x28;&#x29;&#x29;,120&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;prj_day_sale as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;      case when project_code &#x3d; &#x27;GZ_HSH_JH_NQ&#x27; then &#x27;GZ_HSH_JH_ZHT&#x27;&#xd;&#xa;			     else project_code end project_code,&#xd;&#xa;			CONVERT&#x28;VARCHAR&#x28;10&#x29;,BizDate,120&#x29; day_time,&#xd;&#xa;      sum&#x28;PaidAmt&#x29;&#x2f;10000 day_sale&#xd;&#xa;FROM DW_DLDF_ShopSku &#xd;&#xa;WHERE CONVERT&#x28;VARCHAR&#x28;4&#x29;,BizDate,120&#x29; &#x3d; CONVERT&#x28;VARCHAR&#x28;4&#x29;,GETDATE&#x28;&#x29;,120&#x29;  &#xd;&#xa;--CONVERT&#x28;VARCHAR&#x28;7&#x29;,BizDate,120&#x29; &#x3d; &#x27;2019-06&#x27;&#xd;&#xa;GROUP BY project_code,CONVERT&#x28;VARCHAR&#x28;10&#x29;,BizDate,120&#x29;&#xd;&#xa;&#x29;,  --&#x6309;&#x9879;&#x76ee;&#x3001;&#x6309;&#x5929;&#x9500;&#x552e;&#xd;&#xa;&#xd;&#xa;day_cus_flow as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;      case when project_code &#x3d; &#x27;GZ_HSH_JH_NQ&#x27; then &#x27;GZ_HSH_JH_ZHT&#x27;&#xd;&#xa;			     else project_code end project_code,&#xd;&#xa;			CONVERT&#x28;VARCHAR&#x28;10&#x29;,countdate,120&#x29; countdate,&#xd;&#xa;			round&#x28;cast&#x28;insum as decimal&#x29;&#x2f;10000,2&#x29; insum,&#xd;&#xa;			round&#x28;cast&#x28;outsum as decimal&#x29;&#x2f;10000,2&#x29; outsum&#xd;&#xa;FROM YW_YY_FLOW_ANALIST_PROJ_HOUR&#xd;&#xa;WHERE CONVERT&#x28;VARCHAR&#x28;4&#x29;,countdate,120&#x29; &#x3d; CONVERT&#x28;VARCHAR&#x28;4&#x29;,GETDATE&#x28;&#x29;,120&#x29;  &#xd;&#xa;--CONVERT&#x28;VARCHAR&#x28;7&#x29;,countdate,120&#x29; &#x3d; &#x27;2019-06&#x27;&#xd;&#xa;&#x29;  --&#x65e5;&#x5ba2;&#x6d41;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;      t1.project_code,&#xd;&#xa;			t1.project_name,&#xd;&#xa;			t2.the_date day_time,&#xd;&#xa;			isnull&#x28;t3.day_sale,0&#x29; day_sale,&#xd;&#xa;			isnull&#x28;t4.insum,0&#x29; insum,&#xd;&#xa;			isnull&#x28;t4.outsum,0&#x29; outsum,&#xd;&#xa;			GETDATE&#x28;&#x29; as etl_time&#xd;&#xa;FROM orderkey t1&#xd;&#xa;LEFT JOIN rq t2 on t1.the_year &#x3d; t2.the_year&#xd;&#xa;LEFT JOIN prj_day_sale t3 on t1.project_code &#x3d; t3.project_code and t2.the_date &#x3d; t3.day_time  &#xd;&#xa;LEFT JOIN day_cus_flow t4 on t1.project_code &#x3d; t4.project_code and t2.the_date &#x3d; t4.countdate  &#xd;&#xa;ORDER BY day_time</sql>
    <limit>0</limit>
