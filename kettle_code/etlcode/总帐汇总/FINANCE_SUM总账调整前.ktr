    <connection>HS</connection>
    <sql>WITH  XMJB AS&#x28;&#xd;&#xa;SELECT&#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;business,&#xd;&#xa;&#x5b;group&#x5d;,&#xd;&#xa;orderkey&#xd;&#xa;from &#xd;&#xa;YW_HS_PROJECT_BASEINFO&#xd;&#xa;WHERE business IN &#x28;&#x27;&#x5546;&#x573a;&#x27;,&#x27;&#x5199;&#x5b57;&#x697c;&#x27;,&#x27;&#x4ea7;&#x4e1a;&#x56ed;&#x27;&#x29;&#xd;&#xa;and  len&#x28;&#x5b;group&#x5d;&#x29;&#x3c;&#x3e;0&#xd;&#xa;and len&#x28;business&#x29;&#x3c;&#x3e;0 &#xd;&#xa;&#x29;,--&#x9879;&#x76ee;&#x57fa;&#x672c;&#x4fe1;&#x606f;&#xd;&#xa;&#xd;&#xa;sj as &#x28;&#xd;&#xa;select &#xd;&#xa;				a.project_code,&#xd;&#xa;				&#x27;1&#x27; as business,&#xd;&#xa;				b.&#x5b;group&#x5d;,&#xd;&#xa;				a.subject_code,&#xd;&#xa;				a.subject_name,&#xd;&#xa;				a.year,&#xd;&#xa;				a.month,&#xd;&#xa;				sum&#x28;c.&#x5b;value&#x5d;&#x29;&#x2a;10000 as end_balance&#xd;&#xa;				&#xd;&#xa;from YW_YW_BENEFIT_ANALYST_SUM_FINANCE a&#xd;&#xa;full join YW_YW_BENEFIT_ANALYST_SUM_FINANCE c on a.project_code&#x3d;c.project_code and a.subject_code&#x3d;c.subject_code &#xd;&#xa;left join XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;--left join XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;where a.subject_name &#x3d;&#x27;EBITDA1&#x27;&#xd;&#xa;and convert&#x28;varchar&#x28;10&#x29;,dateadd&#x28;MONTH,-1,a.year&#x2b;&#x27;-&#x27;&#x2b;a.month&#x2b;&#x27;-01&#x27;&#x29;,23&#x29;&#x3e;&#x3d;convert&#x28;varchar&#x28;10&#x29;,dateadd&#x28;MONTH,-1,c.year&#x2b;&#x27;-&#x27;&#x2b;c.month&#x2b;&#x27;-01&#x27;&#x29;,23&#x29;&#xd;&#xa;group by a.project_code,b.&#x5b;group&#x5d;,a.subject_code,a.subject_name,a.year,a.month&#xd;&#xa;&#xd;&#xa;UNION ALL&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;&#x27;1&#x27; as business,&#xd;&#xa;b.&#x5b;group&#x5d;,&#xd;&#xa;&#x27;LR02&#x27; as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.&#x5b;year&#x5d; as year,&#xd;&#xa;a.month as month,&#xd;&#xa;sum&#x28;c.value&#x29;&#x2a;10000 as end_balance &#xd;&#xa;FROM YW_HS_ManagementBasic a&#xd;&#xa;LEFT JOIN YW_HS_ManagementBasic c ON a.project_code&#x3d;c.project_code AND a.SX&#x3d;c.SX &#xd;&#xa;AND CONVERT&#x28;VARCHAR&#x28;10&#x29;,CAST&#x28;a.year as varchar&#x29;&#x2b;&#x27;-&#x27;&#x2b;RIGHT&#x28;&#x27;0&#x27;&#x2b;CAST&#x28;a.month as varchar&#x29;,2&#x29;&#x2b;&#x27;-01&#x27;,23&#x29;&#x3e;&#x3d;CONVERT&#x28;VARCHAR&#x28;10&#x29;,CAST&#x28;c.year as varchar&#x29;&#x2b;&#x27;-&#x27;&#x2b;RIGHT&#x28;&#x27;0&#x27;&#x2b;CAST&#x28;c.month as varchar&#x29;,2&#x29;&#x2b;&#x27;-01&#x27;,23&#x29;&#xd;&#xa;left join XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;WHERE a.SX &#x3d;&#x27;6&#x27;&#xd;&#xa;GROUP BY a.project_code,b.&#x5b;group&#x5d;,a.subject_name,a.year,a.month&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select &#xd;&#xa;				a.project_code,&#xd;&#xa;				&#x27;1&#x27; as business,&#xd;&#xa;				b.&#x5b;group&#x5d;,&#xd;&#xa;				a.subject_code,&#xd;&#xa;				a.subject_name,&#xd;&#xa;				a.year,&#xd;&#xa;				a.month,&#xd;&#xa;				a.end_balance as end_balance&#xd;&#xa;from YW_YY_PROFIT_ANALYST a&#xd;&#xa;left join XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;where  a.subject_code in &#x28;&#x27;LR1050&#x27;,&#x27;CW1070&#x27;,&#x27;SJ1090&#x27;,&#x27;KB1130&#x27;,&#x27;KB1140&#x27;,&#x27;LR10501010&#x27;&#x29;&#xd;&#xa;and b.business IN &#x28;&#x27;&#x5546;&#x573a;&#x27;,&#x27;&#x5199;&#x5b57;&#x697c;&#x27;,&#x27;&#x4ea7;&#x4e1a;&#x56ed;&#x27;&#x29;&#xd;&#xa;--&#x5229;&#x6da6;&#x8868;&#x671f;&#x672b;&#x503c;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select &#xd;&#xa;				a.project_code,&#xd;&#xa;				&#x27;1&#x27; as business,&#xd;&#xa;				b.&#x5b;group&#x5d;,&#xd;&#xa;				a.subject_code,&#xd;&#xa;				a.subject_name,&#xd;&#xa;				a.year,&#xd;&#xa;				a.month,&#xd;&#xa;				a.end_balance as end_balance&#xd;&#xa;from YW_YY_ASSETS_ANALYST a&#xd;&#xa;left join  XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;where a.subject_code in &#x28;&#x27;FZ102010101010&#x27;,&#x27;ZC1010101010601010&#x27;,&#x27;ZC101010101050&#x27;&#x29;&#xd;&#xa;and b.business IN &#x28;&#x27;&#x5546;&#x573a;&#x27;,&#x27;&#x5199;&#x5b57;&#x697c;&#x27;,&#x27;&#x4ea7;&#x4e1a;&#x56ed;&#x27;&#x29;&#xd;&#xa;--&#x8d44;&#x4ea7;&#x8868;&#x671f;&#x672b;&#x503c;&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select &#xd;&#xa;				a.project_code,&#xd;&#xa;				&#x27;1&#x27; as business,&#xd;&#xa;				b.&#x5b;group&#x5d;,&#xd;&#xa;				&#x27;ZC1010101010601011&#x27; as subject_code,&#xd;&#xa;				a.subject_name,&#xd;&#xa;				a.year,&#xd;&#xa;				a.month,&#xd;&#xa;				a.market_value as end_balance&#xd;&#xa;from YW_YY_ASSETS_ANALYST a&#xd;&#xa;left join  XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;where a.subject_code in &#x28;&#x27;ZC1010101010601010&#x27;&#x29;&#xd;&#xa;and b.business IN &#x28;&#x27;&#x5546;&#x573a;&#x27;,&#x27;&#x5199;&#x5b57;&#x697c;&#x27;,&#x27;&#x4ea7;&#x4e1a;&#x56ed;&#x27;&#x29;&#xd;&#xa;--&#x8d44;&#x4ea7;&#x8868;&#x5e02;&#x503c;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select &#xd;&#xa;				a.project_code,&#xd;&#xa;				&#x27;1&#x27; as business,&#xd;&#xa;				b.&#x5b;group&#x5d;,&#xd;&#xa;				a.subject_code,&#xd;&#xa;				a.subject_name,&#xd;&#xa;				year&#x28;a.yearmonth&#x2b;&#x27;-01&#x27;&#x29; as year,&#xd;&#xa;				month&#x28;a.yearmonth&#x2b;&#x27;-01&#x27;&#x29; as month,&#xd;&#xa;				a.end_balance as end_balance&#xd;&#xa;from YW_YY_CAPITAL_ANALYST a&#xd;&#xa;left join  XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;where  b.business IN &#x28;&#x27;&#x5546;&#x573a;&#x27;,&#x27;&#x5199;&#x5b57;&#x697c;&#x27;,&#x27;&#x4ea7;&#x4e1a;&#x56ed;&#x27;&#x29;&#xd;&#xa;and a.subject_code &#x3c;&#x3e;&#x27;LR1010&#x27;&#xd;&#xa;--&#x8d44;&#x91d1;&#x8868;&#x671f;&#x672b;&#x503c;&#xd;&#xa;&#xd;&#xa;union all &#xd;&#xa;&#xd;&#xa;select &#xd;&#xa;				a.project_code,&#xd;&#xa;				&#x27;1&#x27; as business,&#xd;&#xa;				b.&#x5b;group&#x5d;,&#xd;&#xa;				case when a.subject_type_code &#x3d; &#x27;1010&#x27; then &#x27;101010101020&#x27; else &#x27;102010101020&#x27; end  as subject_code,&#xd;&#xa;				a.subject_name,&#xd;&#xa;				a.year,&#xd;&#xa;				a.month,&#xd;&#xa;				sum&#x28;a.&#x5b;value&#x5d;&#x29; as end_balance&#xd;&#xa;from YW_YY_FINANCING_ANALYST_CUMULATIVE a &#xd;&#xa;left join  XMJB b on a.project_code&#x3d;b.project_code&#xd;&#xa;where a.subject_code in&#x28;&#x27;10101020&#x27;&#x29;&#xd;&#xa;and a.subject_type_code in&#x28;&#x27;1010&#x27;,&#x27;1020&#x27;&#x29;&#xd;&#xa;GROUP BY a.project_code,b.&#x5b;group&#x5d;,a.year,a.month,a.subject_type_code,a.subject_code ,a.subject_type_name,a.subject_name&#xd;&#xa;--&#x53d6;&#x878d;&#x8d44;&#x5229;&#x606f;&#x8868;&#x7d2f;&#x8ba1;&#x6570;&#x636e;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;select &#xd;&#xa;				b.project_code,&#xd;&#xa;				b.&#x5b;year&#x5d;,&#xd;&#xa;				b.&#x5b;month&#x5d;,&#xd;&#xa;				a.category,&#xd;&#xa;				a.category_code,&#xd;&#xa;				a.BI_subject_code,&#xd;&#xa;				a.BI_subject_name,&#xd;&#xa;				sum&#x28;a.proportion&#x2a;b.end_balance&#x29; as &#x5b;value&#x5d;,&#xd;&#xa;				getdate&#x28;&#x29; as timestamp&#xd;&#xa;	&#xd;&#xa;from  sj b&#xd;&#xa;left join YW_HS_DIM_FINANCE_SUM_MAP a on a.subject_code&#x3d;b.subject_code and a.format&#x3d;b.business&#xd;&#xa;where len&#x28;a.BI_subject_code&#x29;&#x3c;&#x3e;0&#xd;&#xa;and a.format&#x3d;1&#xd;&#xa;group by b.project_code,b.&#x5b;year&#x5d;,b.&#x5b;month&#x5d;,a.category,a.category_code,a.BI_subject_code,a.BI_subject_name&#xd;&#xa;</sql>
