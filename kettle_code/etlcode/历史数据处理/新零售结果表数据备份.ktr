 &#x28;&#xd;&#xa;    value FOR attribute  IN&#x28;&#x5b;YFKM7&#x5d;&#x29;&#xd;&#xa;  &#x29; AS UPV&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;--SELECT &#x2a; from yffk_js_data_ok&#xd;&#xa;&#xd;&#xa;,DATA_LAST AS &#x28;--&#x6570;&#x636e;&#x5408;&#x5e76;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;&#xd;&#xa;case when  a.subject_code in&#x28;&#x27;KSKM3&#x27;,&#x27;KSKM4&#x27;,&#x27;KSKM7&#x27;&#x29; then b.Num &#xd;&#xa;     when  a.subject_code in&#x28;&#x27;YFKM7&#x27;&#x29; then c.Num &#xd;&#xa;     else a.value end as value,&#xd;&#xa;GETDATE&#x28;&#x29; as TIMESTAMPS&#xd;&#xa;from data_ok a&#xd;&#xa;LEFT JOIN dkm_js_data_ok b&#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;and a.subject_code&#x3d;b.subject_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;LEFT JOIN yffk_js_data_ok c&#xd;&#xa;on  a.project_code&#x3d;c.project_code&#xd;&#xa;and a.subject_code&#x3d;c.subject_code&#xd;&#xa;and a.year&#x3d;c.year&#xd;&#xa;and a.month&#x3d;c.month&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from DATA_LAST&#xd;&#xa;--&#x3c;&#x3c;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x6240;&#x5f97;&#x7a0e;&#x6309;ebitda&#x6bd4;&#x4f8b;&#x5206;&#x644a; &#x5e76;&#x8ba1;&#x7b97;&#x51c0;&#x5229;&#x6da6;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;,ts_sds as &#x28;&#xd;&#xa;SELECT&#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;a.value,&#xd;&#xa;sum&#x28;a.value&#x29; over&#x28; partition by a.year,a.month &#x29; as sums &#xd;&#xa;from DATA_LAST  a&#xd;&#xa;where a.subject_code in &#x28;&#x27;KSKM7&#x27;,&#x27;YFKM7&#x27;&#x29;&#xd;&#xa;and a.project_code in &#x28;&#x27;YFFK&#x27;,&#x27;KSD_SH&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from ts_sds&#xd;&#xa;,ts_sdsbl as &#x28;--&#x3d;&#x3d;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x2f;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x5408;&#x8ba1;&#x503c;&#x7b97;&#x5206;&#x914d;&#x6bd4;&#x4f8b;&#xd;&#xa;SELECT &#x2a; &#xd;&#xa;,&#xd;&#xa;case when sums is null then 0&#xd;&#xa;when sums&#x3d;0 then 0&#xd;&#xa;else &#xd;&#xa;round&#x28;cast &#x28;value as float&#x29;&#x2f;cast &#x28;sums as float &#x29;,10&#x29;end as ratio&#xd;&#xa;from ts_sds&#xd;&#xa;--ORDER BY year, month&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from ts_sdsbl&#xd;&#xa;,ksbj_sdsbl as &#x28;--&#x3d;&#x3d;&#x5feb;&#x95ea;&#x5e97;&#x5317;&#x4eac;&#x4e0e;&#x7279;&#x5356;&#x4f1a;ebitda&#x548c;ebitda&#x5408;&#x8ba1;&#xd;&#xa;SELECT&#xd;&#xa;--distinct &#xd;&#xa;a. project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.value&#xd;&#xa;,a.project_px&#xd;&#xa;,sum&#x28;a.value&#x29; over&#x28; partition by a.year,a.month &#x29; as sums &#xd;&#xa;from DATA_LAST  a&#xd;&#xa;where a.subject_code in &#x28;&#x27;KSKM7&#x27;&#x29;&#xd;&#xa;and a.project_code in &#x28;&#x27;KSD_BJ&#x27;,&#x27;XLS_TMH&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;--SELECT &#x2a; from ksbj_sdsbl WHERE project_code in &#x28;&#x27;KSD_BJ&#x27;,&#x27;XLS_TMH&#x27;&#x29;&#xd;&#xa;,ksbj_sdsbl_data as &#x28;--ebitda&#x2f;ebitda&#x5408;&#x8ba1;&#x7b97;&#x6bd4;&#x4f8b;&#xd;&#xa;SELECT &#x2a; &#xd;&#xa;,&#xd;&#xa;case when sums is null then 0&#xd;&#xa;when sums&#x3d;0 then 0&#xd;&#xa;else &#xd;&#xa;round&#x28;cast&#x28;value as float&#x29;&#x2f;cast &#x28;sums as float&#x29;,10&#x29; end as ratio&#xd;&#xa;from ksbj_sdsbl&#xd;&#xa;WHERE project_code in &#x28;&#x27;KSD_BJ&#x27;,&#x27;XLS_TMH&#x27;&#x29;--&#x5f85;&#x52a0;&#x7279;&#x5356;&#x4f1a;&#xd;&#xa;--ORDER BY year, month&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from ksbj_sdsbl_data &#xd;&#xa;,ksbj_sds_zhanbi as &#x28;--&#x5317;&#x4eac;&#x548c;&#x7279;&#x5356;&#x4f1a;&#x7a0e;&#x91d1;&#x2a;ebitda&#x5360;&#x6bd4;&#xd;&#xa;SELECT &#xd;&#xa; a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;a.value&#x2a;b.ratio  as sds_zhanbi&#xd;&#xa;from DATA_LAST a&#xd;&#xa;LEFT JOIN ksbj_sdsbl_data b&#xd;&#xa;on&#xd;&#xa;a.project_code&#x3d;b.project_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;where  CHARINDEX&#x28;&#x27;KSKM10&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;and  a.project_code in &#x28;&#x27;KSD_BJ&#x27;,&#x27;XLS_TMH&#x27;&#x29;--&#x5f85;&#x52a0;&#x7279;&#x5356;&#x4f1a;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from ksbj_sds_zhanbi &#xd;&#xa;,sds_zhanbi as &#x28;--&#x3d;&#x3d;&#x3d;--&#x9884;&#x4ed8;&#x8d39;&#x5361;&#x548c;&#x4e0a;&#x6d77;&#x7a0e;&#x91d1;&#x2a;ebitda&#x5360;&#x6bd4;&#xd;&#xa;SELECT &#xd;&#xa; a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;a.value&#x2a;b.ratio  as sds_zhanbi&#xd;&#xa;from DATA_LAST a&#xd;&#xa;LEFT JOIN ts_sdsbl b&#xd;&#xa;on&#xd;&#xa;a.project_code&#x3d;b.project_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;where &#xd;&#xa;&#x28; CHARINDEX&#x28;&#x27;YFKM10&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;OR CHARINDEX&#x28;&#x27;KSKM10&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;&#x29;&#xd;&#xa;and a.project_code in &#x28;&#x27;YFFK&#x27;,&#x27;KSD_SH&#x27;&#x29;&#xd;&#xa;and a.year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and a.month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;, data as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;project_px, &#xd;&#xa;value &#xd;&#xa;from DATA_LAST where subject_code not  in &#x28;&#x27;YFKM10&#x27;,&#x27;KSKM10&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;project_px, &#xd;&#xa;sds_zhanbi &#xd;&#xa;from sds_zhanbi where subject_code in &#x28;&#x27;YFKM10&#x27;,&#x27;KSKM10&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;project_px, &#xd;&#xa;sds_zhanbi  &#xd;&#xa;from ksbj_sds_zhanbi where subject_code in &#x28;&#x27;KSKM10&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x8ba1;&#x7b97;&#x9884;&#x4ed8;&#x8d39;&#x5361;&#x51c0;&#x5229;&#x6da6;&#xd;&#xa;,jlr as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;--&#x27;&#x27; as subject_code,&#xd;&#xa;--&#x27;&#x27; as subject_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;sum&#x28;value&#x29; as value &#xd;&#xa;from data &#xd;&#xa;where year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and subject_code in &#x28;&#x27;YFKM8&#x27;,&#x27;YFKM9&#x27;,&#x27;YFKM11&#x27;,&#x27;YFKM10&#x27;&#xd;&#xa;,&#x27;KSKM8&#x27;,&#x27;KSKM9&#x27;,&#x27;KSKM11&#x27;,&#x27;KSKM10&#x27;&#xd;&#xa;&#x29;--&#x27;YFKM7&#x27;,&#xd;&#xa;GROUP BY project_code,&#xd;&#xa;year,&#xd;&#xa;month&#xd;&#xa;&#x29;&#xd;&#xa;,jlr_ebitda as &#x28;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;--&#x27;&#x27; as subject_code,&#xd;&#xa;--&#x27;&#x27; as subject_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;sum&#x28;value&#x29; as value &#xd;&#xa;from data &#xd;&#xa;where year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and subject_code in &#x28;&#x27;YFKM7&#x27;,&#x27;KSKM7&#x27;&#x29;--&#x27;YFKM7&#x27;,&#xd;&#xa;GROUP BY project_code,&#xd;&#xa;year,&#xd;&#xa;month&#xd;&#xa;&#x29;&#xd;&#xa;,jlr_data as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;case when a.project_code&#x3d;&#x27;YFFK&#x27; then &#x27;YFKM12&#x27;else &#x27;KSKM12&#x27; end as subject_code, &#xd;&#xa;--&#x27;&#x27; as subject_code,&#xd;&#xa;&#x27;&#x51c0;&#x5229;&#x6da6;&#x27; as subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.value-b.value as value&#xd;&#xa;from jlr_ebitda a&#xd;&#xa;INNER JOIN jlr b&#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;&#x29;&#xd;&#xa; ,data_ook as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px, &#xd;&#xa;case when a.subject_code in &#x28;&#x27;YFKM12&#x27;,&#x27;KSKM12&#x27;&#x29; then b.value else a.value end as value&#xd;&#xa;from data a&#xd;&#xa;LEFT JOIN  jlr_data b&#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;and a.subject_code&#x3d;b.subject_code&#xd;&#xa;&#x29;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;value,&#xd;&#xa;GETDATE&#x28;&#x29; as TIMESTAMPS&#xd;&#xa;from &#xd;&#xa;data_ook a&#xd;&#xa;where &#xd;&#xa;year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;--AND PROJECT_CODE &#x3c;&#x3e;&#x27;XLS_TMH&#x27;</sql>
