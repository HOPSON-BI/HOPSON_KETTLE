    <connection>HS</connection>
    <sql>&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x7279;&#x6b8a;&#x8ba1;&#x7b97;&#x7a0e;&#x91d1;&#x9644;&#x52a0;&#x53ca;&#x5176;&#x5b50;&#x7ea7;&#x79d1;&#x76ee;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;&#xd;&#xa;WITH leiji_data as &#x28;--&#x3d;&#x3d;&#x62ff;&#x65b0;&#x96f6;&#x552e;&#x6570;&#x636e;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.&#x5b;year&#x5d;,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;a.project_px,&#xd;&#xa;A.VALUE as value_leiji&#xd;&#xa;from HS.dbo.YW_YY_PROFIT_XLS  a&#xd;&#xa;where year&#x3d;year&#x28;DATEADD&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;MONTH&#x28;DATEADD&#x28;MONTH,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x6309;&#x7167;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x5360;&#x6bd4;&#x8fdb;&#x884c;&#x5206;&#x914d;&#xff08;&#x9884;&#x4ed8;&#x5361;&#x4e0a;&#x6d77;&#x603b;&#x90e8;&#x2f;&#x28;&#x65b0;&#x96f6;&#x552e;&#x4e94;&#x89d2;&#x573a;&#x5feb;&#x95ea;&#x5e97;&#x2b;&#x9884;&#x4ed8;&#x5361;&#x4e0a;&#x6d77;&#x603b;&#x90e8;&#xff09;&#xd;&#xa;,ts_data as &#x28;--&#x3d;&#x3d;&#x9884;&#x4ed8;&#x8d39;&#x5361;&#x548c;&#x5feb;&#x95ea;&#x5e97;&#x4e0a;&#x6d77;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x6570;&#x636e;&#x4ee5;&#x53ca;&#x9879;&#x76ee;&#x5408;&#x8ba1;&#x6570;&#xd;&#xa;SELECT&#xd;&#xa;--distinct &#xd;&#xa;a. project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.value_leiji&#xd;&#xa;,a.project_px&#xd;&#xa;,sum&#x28;a.value_leiji&#x29; over&#x28; partition by a.year,a.month &#x29; as sums &#xd;&#xa;from leiji_data  a&#xd;&#xa;where a.subject_code in &#x28;&#x27;YFKM2&#x27;,&#x27;KSKM1&#x27;&#x29;&#xd;&#xa;and a.project_code in &#x28;&#x27;YFFK&#x27;,&#x27;KSD_SH&#x27;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;,sjbl as &#x28;--&#x3d;&#x3d;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x2f;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x5408;&#x8ba1;&#x503c;&#x7b97;&#x5206;&#x914d;&#x6bd4;&#x4f8b;&#xd;&#xa;SELECT &#x2a; &#xd;&#xa;,&#xd;&#xa;case when sums is null then 0&#xd;&#xa;when sums&#x3d;0 then 0&#xd;&#xa;else &#xd;&#xa;round&#x28;cast &#x28;value_leiji as float&#x29;&#x2f;cast &#x28;sums as float&#x29;,10&#x29;end as ratio&#xd;&#xa;from ts_data&#xd;&#xa;--ORDER BY year, month&#xd;&#xa;&#x29;&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x5feb;&#x95ea;&#x5e97;&#x5317;&#x4eac;&#x7a0e;&#x91d1;&#x53ca;&#x5b50;&#x79d1;&#x76ee;&#x8ba1;&#x7b97;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;,ksbj_ts_data as &#x28;--&#x3d;&#x3d;&#x5feb;&#x95ea;&#x5e97;&#x5317;&#x4eac;&#x4e0e;&#x7279;&#x5356;&#x4f1a;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x548c;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x5408;&#x8ba1;&#xd;&#xa;SELECT&#xd;&#xa;--distinct &#xd;&#xa;a. project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.value_leiji&#xd;&#xa;,a.project_px&#xd;&#xa;,sum&#x28;a.value_leiji&#x29; over&#x28; partition by a.year,a.month &#x29; as sums &#xd;&#xa;from leiji_data  a&#xd;&#xa;where a.subject_code in &#x28;&#x27;KSKM1&#x27;&#x29;&#xd;&#xa;and a.project_code in &#x28;&#x27;KSD_BJ&#x27;,&#x27;XLS_TMH&#x27;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; FROM ksbj_ts_data&#xd;&#xa;,ksbj_sjbl as &#x28;--&#x7ecf;&#x8425;&#x6536;&#x5165;&#x2f;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x5408;&#x8ba1;&#x7b97;&#x6bd4;&#x4f8b;&#xd;&#xa;SELECT &#x2a; &#xd;&#xa;,&#xd;&#xa;case when sums is null then 0&#xd;&#xa;when sums&#x3d;0 then 0&#xd;&#xa;else &#xd;&#xa;round&#x28;cast &#x28;value_leiji as float&#x29;&#x2f;cast &#x28;sums as float&#x29;,10&#x29; end as ratio&#xd;&#xa;from ksbj_ts_data&#xd;&#xa;WHERE project_code in &#x28;&#x27;KSD_BJ&#x27;,&#x27;XLS_TMH&#x27;&#x29;--&#x5f85;&#x52a0;&#x7279;&#x5356;&#x4f1a;&#xd;&#xa;--ORDER BY year, month&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; FROM ksbj_sjbl&#xd;&#xa;,ksbj_shuijin_lj_zhanbi as &#x28;--&#x5317;&#x4eac;&#x548c;&#x7279;&#x5356;&#x4f1a;&#x7a0e;&#x91d1;&#x2a;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x5360;&#x6bd4;&#xd;&#xa;SELECT &#xd;&#xa; a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;a.value_leiji&#x2a;b.ratio  as shijin_lj_zhanbi&#xd;&#xa;from leiji_data a&#xd;&#xa;LEFT JOIN ksbj_sjbl b&#xd;&#xa;on&#xd;&#xa;a.project_code&#x3d;b.project_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;where &#x28;CHARINDEX&#x28;&#x27;KSKM5.8.&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;and a.subject_code&#x3c;&#x3e;&#x27;KSKM5.8.6&#x27;&#xd;&#xa;--OR CHARINDEX&#x28;&#x27;KSKM10&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;&#x29;&#xd;&#xa;and a.project_code in &#x28;&#x27;KSD_BJ&#x27;,&#x27;XLS_TMH&#x27;&#x29;--&#x5f85;&#x52a0;&#x7279;&#x5356;&#x4f1a;&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from ksbj_shuijin_lj_zhanbi&#xd;&#xa;,shuijin_lj_zhanbi as &#x28;--&#x3d;&#x3d;&#x3d;--&#x9884;&#x4ed8;&#x8d39;&#x5361;&#x548c;&#x4e0a;&#x6d77;&#x7a0e;&#x91d1;&#x2a;&#x7ecf;&#x8425;&#x6536;&#x5165;&#x5360;&#x6bd4;&#xd;&#xa;SELECT &#xd;&#xa; a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;a.value_leiji&#x2a;b.ratio  as shijin_lj_zhanbi&#xd;&#xa;from leiji_data a&#xd;&#xa;LEFT JOIN sjbl b&#xd;&#xa;on&#xd;&#xa;a.project_code&#x3d;b.project_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;where &#x28;CHARINDEX&#x28;&#x27;KSKM5.8.&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;or CHARINDEX&#x28;&#x27;YFKM5&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;--OR CHARINDEX&#x28;&#x27;YFKM10&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;--OR CHARINDEX&#x28;&#x27;KSKM10&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#xd;&#xa;&#x29;&#xd;&#xa;and  a.subject_code&#x3c;&#x3e;&#x27;KSKM5.8.6&#x27;&#xd;&#xa;and a.subject_code&#x3c;&#x3e;&#x27;YFKM5.9&#x27;&#xd;&#xa;and a.project_code in &#x28;&#x27;YFFK&#x27;,&#x27;KSD_SH&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;--&#x5176;&#x4ed6;&#x7a0e;&#x8d39; &#x533a;&#x5206;&#x8f85;&#x52a9;   &#x9664;&#x5176;&#x4ed6;&#x7a0e;&#x8d39;&#x5916;&#x6309;&#x6bd4;&#x4f8b;&#x644a;&#x9500;&#xd;&#xa;,shuifei_KSD as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;shijin_lj_zhanbi&#xd;&#xa;from shuijin_lj_zhanbi a&#xd;&#xa;where  a.subject_code in &#x28;&#x27;KSKM5.8.1&#x27;,&#x27;KSKM5.8.2&#x27;,&#x27;KSKM5.8.3&#x27;,&#x27;KSKM5.8.4&#x27;,&#x27;KSKM5.8.5&#x27;&#x29;&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;shijin_lj_zhanbi&#xd;&#xa;from ksbj_shuijin_lj_zhanbi a&#xd;&#xa;where  a.subject_code in &#x28;&#x27;KSKM5.8.1&#x27;,&#x27;KSKM5.8.2&#x27;,&#x27;KSKM5.8.3&#x27;,&#x27;KSKM5.8.4&#x27;,&#x27;KSKM5.8.5&#x27;&#x29;&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.&#x5b;year&#x5d;,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;a.project_px,&#xd;&#xa;a.value_leiji&#xd;&#xa;from leiji_data a&#xd;&#xa;where  a.subject_code &#x3d;&#x27;KSKM5.8.6&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;,shuifei_yffk as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.&#x5b;year&#x5d;,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;a.project_px,&#xd;&#xa;a.value_leiji&#xd;&#xa;from leiji_data a&#xd;&#xa;where  A.subject_code&#x3d;&#x27;YFKM5.9&#x27;&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;shijin_lj_zhanbi&#xd;&#xa;from shuijin_lj_zhanbi a&#xd;&#xa;where  a.subject_code in &#x28;&#x27;YFKM5.1&#x27;,&#x27;YFKM5.2&#x27;,&#x27;YFKM5.3&#x27;,&#x27;YFKM5.4&#x27;,&#x27;YFKM5.5&#x27;,&#x27;YFKM5.6&#x27;,&#x27;YFKM5.7&#x27;,&#x27;YFKM5.8&#x27;&#x29;&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from shuifei_yffk&#xd;&#xa;,shuijin_fujia as &#x28;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;&#x27;KSKM5.8&#x27; as subject_code,&#xd;&#xa;&#x27;&#x7a0e;&#x91d1;&#x53ca;&#x9644;&#x52a0;&#x27; as subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;--a.project_px,&#xd;&#xa;sum&#x28;shijin_lj_zhanbi&#x29; as shijin_lj_zhanbi&#xd;&#xa;from shuifei_KSD a&#xd;&#xa;GROUP BY a.project_code,a.year,&#xd;&#xa;a.month&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;&#x27;YFKM5&#x27; as subject_code,&#xd;&#xa;&#x27;&#x7a0e;&#x91d1;&#x53ca;&#x9644;&#x52a0;&#x27; as subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;--a.project_px,&#xd;&#xa;sum&#x28;value_leiji&#x29; as shijin_lj_zhanbi&#xd;&#xa;from shuifei_yffk a&#xd;&#xa;GROUP BY a.project_code,a.year,&#xd;&#xa;a.month&#xd;&#xa;&#x29;&#xd;&#xa;,shuijin_sj as &#x28;---&#x7a0e;&#x91d1;&#x53ca;&#x9644;&#x52a0;&#x5408;&#x5e76;&#x540e;&#x6570;&#x636e;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;shijin_lj_zhanbi&#xd;&#xa;from shuifei_KSD a&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;value_leiji&#xd;&#xa; from shuifei_yffk a&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;shijin_lj_zhanbi&#xd;&#xa;from shuijin_fujia a&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from  shuijin_sj&#xd;&#xa;&#xd;&#xa;,data_sj as &#x28;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x7a0e;&#x91d1;&#x548c;&#x6240;&#x5f97;&#x7a0e;&#x6309;&#x7167;&#x6bd4;&#x4f8b;&#x644a;&#x9500;&#x540e;&#x7684;&#x6240;&#x6709;&#x79d1;&#x76ee;&#x6570;&#x636e;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;case when &#x28;CHARINDEX&#x28;&#x27;KSKM5.8&#x27;,a.subject_code&#x29;&#x21;&#x3d;0 or CHARINDEX&#x28;&#x27;YFKM5&#x27;,a.subject_code&#x29;&#x21;&#x3d;0&#x29; then b.shijin_lj_zhanbi&#xd;&#xa;     else a.value end as value&#xd;&#xa;from HS.dbo.YW_YY_PROFIT_XLS a&#xd;&#xa;LEFT JOIN shuijin_sj b&#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;and a.subject_code&#x3d;b.subject_code&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from data_sj&#xd;&#xa;,fyze as &#x28;--&#x3d;&#x3d;&#x5feb;&#x95ea;&#x5e97;&#x5206;&#x644a;&#x540e;&#x7684;&#x7a0e;&#x91d1;&#x8ba1;&#x7b97;&#x8d39;&#x7528;&#x603b;&#x989d;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;&#x27;KSKM5&#x27;    as subject_code,&#xd;&#xa;&#x27;&#x8d39;&#x7528;&#x603b;&#x989d;&#x27; as subject_name,&#xd;&#xa;a.&#x5b;year&#x5d;,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;--a.project_px,&#xd;&#xa;sum&#x28;a.value&#x29; as fyze_sj&#xd;&#xa;from data_sj a&#xd;&#xa;where subject_code &#xd;&#xa;in &#x28;&#x27;KSKM5.1&#x27;,&#x27;KSKM5.2&#x27;,&#x27;KSKM5.3&#x27;,&#x27;KSKM5.4&#x27;,&#x27;KSKM5.5&#x27;,&#x27;KSKM5.6&#x27;,&#x27;KSKM5.7&#x27;,&#x27;KSKM5.8&#x27;&#x29;&#xd;&#xa;GROUP BY a.project_code,&#xd;&#xa;a.&#x5b;year&#x5d;,&#xd;&#xa;a.&#x5b;month&#x5d;&#x29;&#xd;&#xa;--SELECT &#x2a; from fyze&#xd;&#xa;,data_ok as &#x28;--&#x3d;&#x3d;&#x3d;&#x7efc;&#x5408;&#x8d39;&#x7528;&#x603b;&#x989d;&#x540e;&#x7684;&#x65b0;&#x96f6;&#x552e;&#x6570;&#x636e;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.project_px,&#xd;&#xa;case when a.subject_code&#x3d;&#x27;KSKM5&#x27; then c.fyze_sj&#xd;&#xa;     else a.value end as value&#xd;&#xa;from data_sj a&#xd;&#xa;LEFT JOIN fyze c &#xd;&#xa;on a.project_code&#x3d;C.project_code&#xd;&#xa;and a.subject_code&#x3d;C.subject_code&#xd;&#xa;and a.year&#x3d;C.year&#xd;&#xa;and a.month&#x3d;C.month&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from data_ok where subject_code&#x3d;&#x27;YFKM3&#x27;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;--SELECT &#x2a; from data_sj WHERE project_code&#x3d;&#x27;KSD_BJ&#x27; and month&#x3d;&#x27;4&#x27; and SUBJECT_CODE&#x3d;&#x27;KSKM5&#x27;&#xd;&#xa;,dkm_js as &#x28; --&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x5feb;&#x95ea;&#x5e97;&#x7279;&#x6b8a;&#x8ba1;&#x7b97;KSKM3 &#x6bdb;&#x5229;&#x3d;&#x9500;&#x8d27;&#x6536;&#x5165;-&#x9500;&#x8d27;&#x6210;&#x672c;&#xd;&#xa;SELECT &#xd;&#xa;&#x2a;&#xd;&#xa; from &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;subject_code,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;--project_px,&#xd;&#xa;value&#xd;&#xa;from data_ok &#xd;&#xa;where project_code &#x3c;&#x3e;&#x27;YFFK&#x27;&#xd;&#xa;and subject_code in &#x28;&#x27;KSKM1.1&#x27;,&#x27;KSKM2&#x27;,&#x27;KSKM1&#x27;,&#x27;KSKM5&#x27;,&#x27;KSKM6&#x27;,&#x27;KSKM8&#x27;,&#x27;KSKM9&#x27;,&#x27;KSKM11&#x27;&#x29;&#xd;&#xa;&#x29; C&#xd;&#xa;PIVOT&#x28;sum&#x28;value&#x29; FOR &#x5b;subject_code&#x5d; IN&#x28;&#x5b;KSKM1.1&#x5d;,&#x5b;KSKM2&#x5d;,&#x5b;KSKM1&#x5d;,&#x5b;KSKM5&#x5d;,&#x5b;KSKM6&#x5d;,&#x5b;KSKM8&#x5d;,&#x5b;KSKM9&#x5d;,&#x5b;KSKM11&#x5d;&#x29;&#x29; AS T --,&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from dkm_js WHERE project_code&#x3d;&#x27;KSD_BJ&#x27; and month&#x