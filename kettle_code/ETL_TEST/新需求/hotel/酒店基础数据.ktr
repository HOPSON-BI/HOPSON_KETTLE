    <sql> --&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;EAS&#x9152;&#x5e97;&#x6570;&#x636e;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;&#xd;&#xa;with subject1 as&#x28;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x79d1;&#x76ee;&#x5168;&#x8def;&#x5f84;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;select&#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.upsubject_code,&#xd;&#xa;a.level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;CAST&#x28;a.subject_code&#x2b;&#x27;,&#x27; AS VARCHAR&#x28;max&#x29;&#x29; AS fullpath&#xd;&#xa;from YW_HS_DIM_SUBJECT_HOTEL_MAP a&#xd;&#xa;where len&#x28;a.upsubject_code&#x29; &#x3d; 0&#xd;&#xa;and a.ndmark&#x3d;&#x27;2019&#x27;&#xd;&#xa;and a.yetaicode&#x3d;&#x27;2&#x27;&#xd;&#xa;union all&#xd;&#xa;select&#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.upsubject_code,&#xd;&#xa;a.level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;cast&#x28;subject1.fullpath &#x2b; &#x27;-&#x3e;&#x27;&#x2b; a.subject_code&#x2b;&#x27;,&#x27;  as VARCHAR&#x28;max&#x29;&#x29;as fullpath&#xd;&#xa;from YW_HS_DIM_SUBJECT_HOTEL_MAP a&#xd;&#xa;inner join subject1  on subject1.subject_code &#x3d; a.upsubject_code&#xd;&#xa;where  a.ndmark&#x3d;&#x27;2019&#x27;&#xd;&#xa;and a.yetaicode&#x3d;&#x27;2&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from subject1&#xd;&#xa;,project as &#x28;--&#x9879;&#x76ee;&#xd;&#xa;SELECT distinct&#x28;a.project_code&#x29;,a.project_name,b.&#x5b;group&#x5d; FROM HS.DBO.&#x5b;YW_HS_NC_BI_PROJECT_MAP&#x5d; a&#xd;&#xa;LEFT JOIN HS.DBO.YW_HS_PROJECT_BASEINFO_ZZ b ON a.project_code&#x3d;b.project_code &#xd;&#xa;WHERE b.business IN&#x28;&#x27;&#x9152;&#x5e97;&#x27;&#x29;&#xd;&#xa;AND len&#x28;a.project_code&#x29; &#x3e;0&#xd;&#xa;--and b.project_code  in &#x28;&#x27;FS_JD_KYJX&#x27;&#x29;&#xd;&#xa;UNION ALL &#xd;&#xa;SELECT distinct&#x28;a.project_code&#x29;,a.project_name,&#x27;&#x5408;&#x751f;&#x27; as &#x5b;group&#x5d; FROM hs.dbo.&#x5b;YW_HS_EAS_BI_PROJECT_MAP&#x5d; a&#xd;&#xa;LEFT JOIN hs.dbo.YW_HS_PROJECT_BASEINFO_ZZ b &#xd;&#xa;ON a.project_code&#x3d;b.project_code &#xd;&#xa;WHERE b.business IN&#x28;&#x27;&#x9152;&#x5e97;&#x27;,&#x27;&#x9ad8;&#x5c14;&#x592b;&#x27;,&#x27;&#x535a;&#x7269;&#x9986;&#x27;&#x29;&#xd;&#xa;AND a.project_code  in&#x28;&#x27;GZ_JD_ZJDJ&#x27;,&#x27;TY_JD_TYJD&#x27;,&#xd;&#xa;&#x27;TJ_JD_BDDJC&#x27;,&#x27;SH_JD_WJCKY&#x27;,&#x27;SJQBS&#x27;,&#x27;TJ_WQG&#x27;,&#x27;BWG&#x27;,&#x27;TJ_JD_KY&#x27;,&#x27;TJ_JD_SSHG&#x27;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;,shijian as &#x28;--&#x65f6;&#x95f4;&#x5168;&#x5339;&#x914d;&#xd;&#xa;SELECT DISTINCT the_year as year, month&#x28;CONVERT&#x28;varchar, the_date, 111&#x29;&#x29; as month from Dim_time &#xd;&#xa;where the_year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x28;the_date&#x29;&#x3d;month&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;,subject as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;&#x2a;&#xd;&#xa;from subject1 c&#xd;&#xa;  --where project_code in &#x28;&#x27;KSD_SH&#x27;,&#x27;YFFK&#x27;&#x29;--ORDER BY project_code,project_px&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from subject WHERE subject_code&#x3d;&#x27;SR101010201030&#x27;&#xd;&#xa;,project_shijian as &#x28;&#xd;&#xa;SELECT&#xd;&#xa;a.&#x2a;,b.&#x2a;&#xd;&#xa;from project a&#xd;&#xa;LEFT JOIN shijian b&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;--SELECT &#x2a; from subject where subject_code&#x3d;&#x27;DSYZ2.4.19&#x27;&#xd;&#xa;,eas_km_sj as &#x28;--eas&#x672b;&#x7ea7;&#x4f1a;&#x8ba1;&#x79d1;&#x76ee;&#xd;&#xa;select &#xd;&#xa; a.project_code,&#xd;&#xa;	a.project_name,&#xd;&#xa;	a.subject_code,&#xd;&#xa;	a.subject_name,&#xd;&#xa;	ISNULL&#x28;a.assistprojectnum,1&#x29; AS assistprojectnum,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;code&#xd;&#xa;	ISNULL&#x28;a.assistprojecttype,1&#x29; AS assistprojecttype,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.assistprojectname,1&#x29; AS assistprojectname,   --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7684;&#x540d;&#x79f0;&#xd;&#xa;	a.year,&#xd;&#xa;	a.month,&#xd;&#xa;	case when CHARINDEX&#x28;&#x27;1&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_J &#xd;&#xa;     when CHARINDEX&#x28;&#x27;2&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_D  &#xd;&#xa;     ELSE a.period_fs end as sj&#xd;&#xa;from YW_YY_EAS_ACCOUNTBALANCE a&#xd;&#xa;WHERE project_code IN &#x28;SELECT DISTINCT project_code as project_code FROM project where &#x5b;group&#x5d;&#x3d;&#x27;&#x5408;&#x751f;&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from eas_km_sj where project_code&#x3d;&#x27;GZ_JD_ZJDJ&#x27;and month&#x3d;&#x27;4&#x27; and subject_code in &#x28;&#x27;6051.05.07&#x27;,&#x27;6051.05.09&#x27;&#x29;&#xd;&#xa;,eas_fzz_sj as&#x28;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;  a.project_code,&#xd;&#xa;	a.project_name,&#xd;&#xa;	a.subject_code,&#xd;&#xa;	a.subject_name,&#xd;&#xa;	ISNULL&#x28;a.assistprojectnum,1&#x29; AS assistprojectnum,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;code&#xd;&#xa;	ISNULL&#x28;a.assistprojecttype,1&#x29; AS assistprojecttype,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.assistprojectname,1&#x29; AS assistprojectname ,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7684;&#x540d;&#x79f0;&#xd;&#xa;  ISNULL&#x28;a.Fcontracttype_num,1&#x29; AS Fcontracttype_num ,--&#x8f85;&#x52a9;&#x5408;&#x540c;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.Fcontracttype_name,1&#x29; AS Fcontracttype_name,--&#x8f85;&#x52a9;&#x5408;&#x540c;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;  ISNULL&#x28;a.Fprojectperiod_name,1&#x29; AS Fprojectperiod_name,&#xd;&#xa;  ISNULL&#x28;a.Fprojectperiod_num,1&#x29; AS Fprojectperiod_num,&#xd;&#xa;	a.year,&#xd;&#xa;	a.month,&#xd;&#xa;	case when CHARINDEX&#x28;&#x27;1&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_J &#xd;&#xa;     when CHARINDEX&#x28;&#x27;2&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_D  &#xd;&#xa;     ELSE a.period_fs end as sj&#xd;&#xa;from YW_YY_EAS_ASSISTBALANCE a&#xd;&#xa;WHERE project_code IN &#x28;SELECT DISTINCT project_code as project_code FROM project where &#x5b;group&#x5d;&#x3d;&#x27;&#x5408;&#x751f;&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a;from eas_fzz_sj where  project_code&#x3d;&#x27;GZ_JD_ZJDJ&#x27;&#xd;&#xa;,eas_km_last as&#x28;&#xd;&#xa;SELECT &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;a.zhuti,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;	sum&#x28;a.fx&#x2a; isnull&#x28;b.sj,0&#x29;&#x29; as  sj&#xd;&#xa;from YW_HS_SUBJECT_KJKM_HOTEL_MAP a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN eas_km_sj b &#xd;&#xa;on ltrim&#x28;rtrim&#x28;a.kj_code&#x29;&#x29;&#x3d;b.subject_code&#xd;&#xa;and a.assistprojecttype&#x3d;b.assistprojecttype&#xd;&#xa;and a.assistprojectname&#x3d;b.assistprojectname&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;and c.project_code&#x3d;b.project_code&#xd;&#xa;where a.source&#x3d;&#x27;km&#x27;&#xd;&#xa;AND C.&#x5b;GROUP&#x5d;&#x3d;&#x27;&#x5408;&#x751f;&#x27;&#xd;&#xa;and a.syssource&#x3d;&#x27;EAS&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;--b.subject_code as kj_code,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;a.zhuti&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;a.zhuti,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;	sum&#x28;a.fx&#x2a; isnull&#x28;b.sj,0&#x29;&#x29; as  sj&#xd;&#xa;from YW_HS_SUBJECT_KJKM_HOTEL_MAP a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN eas_fzz_sj b &#xd;&#xa;on &#xd;&#xa; charindex&#x28;ltrim&#x28;rtrim&#x28;a.kj_code&#x29;&#x29;,b.subject_code&#x29;&#x3d;1&#xd;&#xa;and a.assistprojecttype&#x3d;b.assistprojecttype&#xd;&#xa;and a.assistprojectname&#x3d;b.assistprojectname&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;and c.project_code&#x3d;b.project_code&#xd;&#xa;where a.source&#x3d;&#x27;fzz&#x27;&#xd;&#xa;AND C.&#x5b;GROUP&#x5d;&#x3d;&#x27;&#x5408;&#x751f;&#x27;&#xd;&#xa;and a.syssource&#x3d;&#x27;EAS&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;a.zhuti&#xd;&#xa;&#x29; &#xd;&#xa;--SELECT &#x2a;from eas_km_last where  project_code&#x3d;&#x27;SH_JD_WJCKY&#x27;and month&#x3d;&#x27;4&#x27; and  subject_code&#x3d;&#x27;SR1010102010301030&#x27;&#xd;&#xa;,eas_km_last_ok as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;a.zhuti,&#xd;&#xa;--case when a.subject_code&#x3d;&#x27;SR101010201030&#x27; then &#x27;SR101010201030,&#x27; else b.fullpath end as fullpath ,&#xd;&#xa;b.fullpath,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.sj&#xd;&#xa;from eas_km_last a&#xd;&#xa;INNER JOIN subject b &#xd;&#xa;on a.subject_code&#x3d;b.subject_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from eas_km_last_ok where  project_code&#x3d;&#x27;SH_JD_WJCKY&#x27;and month&#x3d;&#x27;4&#x27; and  subject_code&#x3d;&#x27;SR101010201030&#x27;&#xd;&#xa;,eas_jbsj as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;c. project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.level,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;b.zhuti,&#xd;&#xa;sum&#x28;b.sj&#x29; as sj&#xd;&#xa;from subject a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN eas_km_last_ok b&#xd;&#xa;on charindex&#x28;a.fullpath,b.fullpath&#x29;&#x3d;1&#xd;&#xa;and c.project_code&#x3d;b.project_code&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;where c.&#x5b;group&#x5d;&#x3d;&#x27;&#x5408;&#x751f;&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c. project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.level,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;b.zhuti&#xd;&#xa;&#x29;&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;NC&#x9879;&#x76ee;&#x6570;&#x636e;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;,nc_km_sj as &#x28;--eas&#x672b;&#x7ea7;&#x4f1a;&#x8ba1;&#x79d1;&#x76ee;&#xd;&#xa;select &#xd;&#xa; a.project_code,&#xd;&#xa;	a.project_name,&#xd;&#xa;	a.subject_code,&#xd;&#xa;	a.subject_name,&#xd;&#xa;	ISNULL&#x28;a.assistprojectnum,1&#x29; AS assistprojectnum,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;code&#xd;&#xa;	ISNULL&#x28;a.assistprojecttype,1&#x29; AS assistprojecttype,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.assistprojectname,1&#x29; AS assistprojectname,   --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7684;&#x540d;&#x79f0;&#xd;&#xa;	a.year,&#xd;&#xa;	a.month,&#xd;&#xa;	case when CHARINDEX&#x28;&#x27;1&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_J &#xd;&#xa;     when CHARINDEX&#x28;&#x27;2&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_D  &#xd;&#xa;     ELSE a.period_fs end as sj&#xd;&#xa;from YW_YY_NC_ACCOUNTBALANCE a&#xd;&#xa;WHERE project_code IN &#x28;SELECT DISTINCT project_code as project_code FROM project where &#x5b;group&#x5d;&#x3d;&#x27;&#x73e0;&#x6c5f;&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from nc_km_sj where project_code&#x3d;&#x27;GZ_JD_ZJGJ&#x27;and month&#x3d;&#x27;4&#x27; and subject_code in &#x28;&#x27;6051.05.07&#x27;,&#x27;6051.05.09&#x27;&#x29;&#xd;&#xa;,nc_fzz_sj as&#x28;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;  a.project_code,&#xd;&#xa;	a.project_name,&#xd;&#xa;	a.subject_code,&#xd;&#xa;	a.subject_name,&#xd;&#xa;	ISNULL&#x28;a.assistprojectnum,1&#x29; AS assistprojectnum,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;code&#xd;&#xa;	ISNULL&#x28;a.assistprojecttype,1&#x29; AS assistprojecttype,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.assistprojectname,1&#x29; AS assistprojectname ,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7684;&#x540d;&#x79f0;&#xd;&#xa;  --ISNULL&#x28;a.Fcontracttype_num,1&#x29; AS Fcontracttype_num ,--&#x8f85;&#x52a9;&#x5408;&#x540c;&#x7c7b;&#x578b;&#xd;&#xa;	--ISNULL&#x28;a.Fcontracttype_name,1&#x29; AS Fcontracttype_name,--&#x8f85;&#x52a9;&#x5408;&#x540c;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;  --ISNULL&#x28;a.Fprojectperiod_name,1&#x29; AS Fprojectperiod_name,&#xd;&#xa;  --ISNULL&#x28;a.Fprojectperiod_num,1&#x29; AS Fprojectperiod_num,&#xd;&#xa;  isnull&#x28;a.BMFzhs,1&#x29; as BMFzhs ,&#xd;&#xa;	a.year,&#xd;&#xa;	a.month,&#xd;&#xa;	case when CHARINDEX&#x28;&#x27;1&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_J &#xd;&#xa;     when CHARINDEX&#x28;&#x27;2&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_D  &#xd;&#xa;     ELSE a.period_fs end as sj&#xd;&#xa;from YW_YY_NC_ASSISTBALANCE a&#xd;&#xa;WHERE project_code IN &#x28;SELECT DISTINCT project_code as project_code FROM project where &#x5b;group&#x5d;&#x3d;&#x27;&#x73e0;&#x6c5f;&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a;from nc_fzz_sj   where   project_code&#x3d;&#x27;SH_FJY_KYJXJD&#x27;and month&#x3d;&#x27;4&#x27;&#xd;&#xa;--and subject_code like &#x27;60510507&#x25;&#x27;&#xd;&#xa;,nc_km_last as&#x28;&#xd;&#xa;SELECT &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;a.zhuti,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;	sum&#x28;a.fx&#x2a; isnull&#x28;b.sj,0&#x29;&#x29; as  sj&#xd;&#xa;from YW_HS_SUBJECT_KJKM_HOTEL_MAP a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN nc_km_sj b &#xd;&#xa;on ltrim&#x28;rtrim&#x28;a.kj_code&#x29;&#x29;&#x3d;b.subject_code&#xd;&#xa;and a.assistprojecttype&#x3d;b.assistprojecttype&#xd;&#xa;and a.assistprojectname&#x3d;b.assistprojectname&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;and c.project_code&#x3d;b.project_code&#xd;&#xa;where a.source&#x3d;&#x27;km&#x27;&#xd;&#xa;AND C.&#x5b;GROUP&#x5d;&#x3d;&#x27;&#x73e0;&#x6c5f;&#x27;&#xd;&#xa;and a.syssource&#x3d;&#x27;NC&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;--b.subject_code as kj_code,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;a.zhuti&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;a.zhuti,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;	sum&#x28;a.fx&#x2a; isnull&#x28;b.sj,0&#x29;&#x29; as  sj&#xd;&#xa;from YW_HS_SUBJECT_KJKM_HOTEL_MAP a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN nc_fzz_sj b &#xd;&#xa;on &#xd;&#xa; charindex&#x28;ltrim&#x28;rtrim&#x28;a.kj_code&#x29;&#x29;,b.subject_code&#x29;&#x3d;1&#xd;&#xa;and a.assistprojecttype&#x3d;b.assistprojecttype&#xd;&#xa;and a.assistprojectname&#x3d;replace&#x28;b.BMFzhs,&#x27;&#x65b0;&#x27;,&#x27;&#x27;&#x29;&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;and c.project_code&#x3d;b.project_code&#xd;&#xa;where a.source&#x3d;&#x27;fzz&#x27;&#xd;&#xa;AND C.&#x5b;GROUP&#x5d;&#x3d;&#x27;&#x73e0;&#x6c5f;&#x27;&#xd;&#xa;and a.syssource&#x3d;&#x27;NC&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c.project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;a.zhuti&#xd;&#xa;&#x29; &#xd;&#xa;--SELECT &#x2a;from nc_km_last where  project_code&#x3d;&#x27;SH_FJY_KYJXJD&#x27;and month&#x3d;&#x27;4&#x27; and  subject_code&#x3d;&#x27;SR10101160&#x27;&#xd;&#xa;,nc_km_last_ok as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--a.kj_code,&#xd;&#xa;a.zhuti,&#xd;&#xa;--case when a.subject_code&#x3d;&#x27;SR101010201030&#x27; then &#x27;SR101010201030,&#x27; else b.fullpath end as fullpath ,&#xd;&#xa;b.fullpath,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.sj&#xd;&#xa;from nc_km_last a&#xd;&#xa;INNER JOIN subject b &#xd;&#xa;on a.subject_code&#x3d;b.subject_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from nc_km_last_ok where  project_code&#x3d;&#x27;SH_FJY_KYJXJD&#x27;and month&#x3d;&#x27;4&#x27; and  subject_code&#x3d;&#x27;SR10101160&#x27;&#xd;&#xa;,nc_jbsj as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;c. project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.level,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;b.zhuti,&#xd;&#xa;sum&#x28;b.sj&#x29; as sj&#xd;&#xa;from subject a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN nc_km_last_ok b&#xd;&#xa;on charindex&#x28;a.fullpath,b.fullpath&#x29;&#x3d;1&#xd;&#xa;and c.project_code&#x3d;b.project_code&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;where c.&#x5b;group&#x5d;&#x3d;&#x27;&#x73e0;&#x6c5f;&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c. project_code,&#xd;&#xa;c.project_name,&#xd;&#xa;a.level,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;b.zhuti&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from nc_jbsj where  project_code&#x3d;&#x27;SH_FJY_KYJXJD&#x27;and month&#x3d;&#x27;4&#x27; and  subject_code&#x3d;&#x27;SR10101160&#x27;&#xd;&#xa;,jbsj as &#x28;&#xd;&#xa;SELECT &#x2a; from eas_jbsj&#xd;&#xa;union all &#xd;&#xa;SELECT &#x2a; from nc_jbsj&#xd;&#xa;&#x29;&#xd;&#xa;,GOP AS &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;zhuti,&#xd;&#xa;ISNULL&#x28;a.SR1010,0&#x29; as SR1010,&#xd;&#xa;ISNULL&#x28;a.ZC1030,0&#x29; as ZC1030,&#xd;&#xa;ISNULL&#x28;a.ZC1020,0&#x29; as ZC1020,&#xd;&#xa;ISNULL&#x28;a.ZC1040,0&#x29; as ZC1040,&#xd;&#xa;ISNULL&#x28;a.ZC1050,0&#x29; as ZC1050,&#xd;&#xa;ISNULL&#x28;a.CW1060,0&#x29; AS CW1060,&#xd;&#xa;ISNULL&#x28;a.DSDF1070,0&#x29; AS DSDF1070,&#xd;&#xa;ISNULL&#x28;a.ZC1070,0&#x29; AS ZC1070,&#xd;&#xa;ISNULL&#x28;a.TZZC1110,0&#x29; as TZZC1110,&#xd;&#xa;ISNULL&#x28;ZC1135,0&#x29; AS ZC1135,&#xd;&#xa;ISNULL&#x28;ZC11401040,0&#x29;AS ZC11401040 ,&#xd;&#xa;ISNULL&#x28;ZC11401035,0&#x29;AS ZC11401035,&#xd;&#xa;ISNULL&#x28;ZC11401050,0&#x29;AS ZC11401050,&#xd;&#xa;ISNULL&#x28;ZC11401060,0&#x29;AS ZC11401060,&#xd;&#xa;ISNULL&#x28;ZC10401440,0&#x29;AS ZC10401440&#xd;&#xa;from &#x28;&#xd;&#xa;SELECT &#x2a; from &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;subject_code,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;zhuti,&#xd;&#xa;ISNULL&#x28;sj,0&#x29;AS period_J&#xd;&#xa;from jbsj&#xd;&#xa;where subject_code in &#x28;&#x27;SR1010&#x27;, &#x27;ZC1030&#x27;,&#x27;ZC1020&#x27;,&#x27;ZC1040&#x27;,&#x27;ZC1050&#x27;,&#x27;CW1060&#x27;,&#x27;ZC1070&#x27;,&#xd;&#xa;&#x27;DSDF1070&#x27;,&#x27;TZZC1110&#x27;,&#x27;ZC1135&#x27;,&#x27;ZC11401040&#x27;,&#x27;ZC11401035&#x27;,&#x27;ZC11401050&#x27;,&#x27;ZC11401060&#x27;,&#x27;ZC10401440&#x27;&#x29;&#xd;&#xa;&#x29;C&#xd;&#xa;PIVOT&#x28;sum&#x28;period_J&#x29; FOR &#x5b;subject_code&#x5d; IN&#x28;&#x5b;SR1010&#x5d;,&#x5b;ZC1030&#x5d;,&#x5b;ZC1020&#x5d;,&#x5b;ZC1040&#x5d;,&#x5b;ZC1050&#x5d;,&#xd;&#xa;&#x5b;CW1060&#x5d;,&#x5b;DSDF1070&#x5d;,&#x5b;ZC1070&#x5d;,&#x5b;TZZC1110&#x5d;,&#x5b;ZC1135&#x5d;,&#xd;&#xa;&#x5b;ZC11401040&#x5d;,&#xd;&#xa;&#x5b;ZC11401035&#x5d;,&#xd;&#xa;&#x5b;ZC11401050&#x5d;,&#xd;&#xa;&#x5b;ZC11401060&#x5d;,&#x5b;ZC10401440&#x5d;&#x29;&#x29; AS T &#xd;&#xa;&#x29; a&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; FROM GOP where  project_code&#x3d;&#x27;TJ_JD_BDDJC&#x27;and month&#x3d;&#x27;4&#x27; &#xd;&#xa;,GOP_DATA_S AS &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;zhuti,&#xd;&#xa; &#x5b;SR1010&#x5d;-&#x5b;ZC1030&#x5d;-&#x5b;ZC1020&#x5d;-&#x5b;ZC1040&#x5d;-&#x5b;ZC1050&#x5d;-&#x5b;CW1060&#x5d;-&#x5b;DSDF1070&#x5d;-&#x5b;ZC1070&#x5d;-&#x5b;ZC10401440&#x5d;AS LR1080,&#xd;&#xa;&#x5b;SR1010&#x5d;-&#x5b;ZC1030&#x5d;-&#x5b;ZC1020&#x5d;-&#x5b;ZC1040&#x5d;-&#x5b;ZC1050&#x5d;-&#x5b;CW1060&#x5d;-&#x5b;DSDF1070&#x5d;-&#x5b;ZC1070&#x5d;-&#x5b;TZZC1110&#x5d;-&#x5b;ZC10401440&#x5d; AS LR1120,&#xd;&#xa;&#x28;&#x5b;SR1010&#x5d;-&#x5b;ZC1030&#x5d;-&#x5b;ZC1020&#x5d;-&#x5b;ZC1040&#x5d;-&#x5b;ZC1050&#x5d;-&#x5b;CW1060&#x5d;-&#x5b;DSDF1070&#x5d;&#xd;&#xa;-&#x5b;ZC1070&#x5d;-&#x5b;TZZC1110&#x5d;-&#x5b;ZC1135&#x5d;-&#x5b;ZC11401040&#x5d;-&#x5b;ZC11401035&#x5d;-&#x5b;ZC11401050&#x5d;-&#x5b;ZC11401060&#x5d;-&#x5b;ZC10401440&#x5d; &#x29;AS LR1150&#xd;&#xa;FROM  GOP &#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; FROM GOP where  project_code&#x3d;&#x27;SH_JD_WJCKY&#x27;and month&#x3d;&#x27;4&#x27;&#xd;&#xa;,GOP_DATA AS &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;subject_code&#x3d;attribute, &#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;zhuti,&#xd;&#xa;Num&#x3d;period_J&#xd;&#xa;from GOP_DATA_S &#xd;&#xa;UNPIVOT&#xd;&#xa;  &#x28;&#xd;&#xa;    period_J FOR attribute  IN&#x28;&#x5b;LR1080&#x5d;,&#x5b;LR1120&#x5d;,&#x5b;LR1150&#x5d;&#x29;&#xd;&#xa;  &#x29; AS UPV&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from GOP_DATA where  project_code&#x3d;&#x27;TJ_JD_BDDJC&#x27;and month&#x3d;&#x27;4&#x27;&#xd;&#xa;,data_ok as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.project_name,&#xd;&#xa;a.&#x5b;level&#x5d;,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;a.year,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;case when a.subject_code  in &#x28;&#x27;LR1080&#x27;,&#x27;LR1120&#x27;,&#x27;LR1150&#x27;&#x29;  then b.zhuti ELSE a.zhuti end as zhuti,&#xd;&#xa;case when a.subject_code  in &#x28;&#x27;LR1080&#x27;,&#x27;LR1120&#x27;,&#x27;LR1150&#x27;&#x29;  then b.Num else a.sj end as sj&#xd;&#xa;from jbsj a&#xd;&#xa;left JOIN GOP_DATA  b&#xd;&#xa;on  a.project_code&#x3d;b.project_code&#xd;&#xa;and a.subject_code&#x3d;b.subject_code&#xd;&#xa;--and a.zhuti&#x3d;b.zhuti&#xd;&#xa;and a.year&#x3d;b.year&#xd;&#xa;and a.month&#x3d;b.month&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;&#x27;JD_TJ_QTZC&#x27; as project_code,&#xd;&#xa;&#x27;&#x4eac;&#x6d25;&#x65b0;&#x57ce;&#x5176;&#x4ed6;&#x8d44;&#x4ea7;&#x27; as project_name,&#xd;&#xa;a.&#x5b;level&#x5d;,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;a.year,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;a.zhuti,&#xd;&#xa;sum&#x28;a.sj&#x29; as sj&#xd;&#xa; from data_ok a &#xd;&#xa;where a.project_code in &#x28;&#x27;SJQBS&#x27;,&#x27;BWG&#x27;,&#x27;TJ_WQG&#x27;&#x29;&#xd;&#xa;and   a.year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and a.month&#x3d;month&#x28;dateadd&#x28;month,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;GROUP BY &#xd;&#xa;a.&#x5b;level&#x5d;,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;a.year,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;a.zhuti&#xd;&#xa;&#xd;&#xa;union ALL&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.project_name,&#xd;&#xa;a.&#x5b;level&#x5d;,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;a.year,&#xd;&#xa;a.&#x5b;month&#x5d;,&#xd;&#xa;a.zhuti,&#xd;&#xa;a.sj &#xd;&#xa; from data_ok a&#xd;&#xa;WHERE  a.year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and a.month&#x3d;month&#x28;dateadd&#x28;month,-1,GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#xd;&#xa;--where  project_code&#x3d;&#x27;TJ_JD_BDDJC&#x27;and month&#x3d;&#x27;4&#x27; and subject_code&#x3d;&#x27;LR1080&#x27;&#xd;&#xa;</sql>
