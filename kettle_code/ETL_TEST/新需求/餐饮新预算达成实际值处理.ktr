    <sql>with project as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;DISTINCT&#xd;&#xa;--b.FOrgUnitNum,&#xd;&#xa;d.project_code as zproject_code,&#xd;&#xa;d.short_name as zproject_name ,&#xd;&#xa;b.project_code ,&#xd;&#xa;b.project_name &#xd;&#xa;FROM dbo.&#x5b;YW_HS_EAS_BI_PROJECT_MAP&#x5d;  a&#xd;&#xa;inner JOIN dbo.&#x5b;YW_HS_EAS_BI_PROJECT_MAP&#x5d;  b&#xd;&#xa;on a.FOrgUnitNum&#x3d;b.FOrgUnitNum&#xd;&#xa;INNER JOIN YW_HS_PROJECT_BASEINFO_ZZ c&#xd;&#xa; ON a.project_code&#x3d;c.project_code &#xd;&#xa;INNER JOIN YW_HS_PROJECT_BASEINFO d&#xd;&#xa;on a.project_code&#x3d;d.project_code&#xd;&#xa;WHERE &#xd;&#xa;  c.business IN&#x28;&#x27;&#x9910;&#x996e;&#x27;&#x29;&#xd;&#xa;and d.branch&#x3d;&#x27;&#x9910;&#x996e;&#x677f;&#x5757;&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from project&#xd;&#xa;,month_actual as &#x28;&#xd;&#xa;SELECT &#xd;&#xa; b.zproject_code as project_code,&#xd;&#xa; b.zproject_name as project_name,&#xd;&#xa; a.subject_code,&#xd;&#xa; a.subject_name,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;sum&#x28;isnull&#x28;listed_actual,0&#x29;&#x29; as org_lj_actual&#xd;&#xa;from YW_YY_PROFIT_LIST_ANALYST_CY a&#xd;&#xa;INNER JOIN PROJECT b&#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;WHERE a.year&#x3d;year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and a.month&#x3d;month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and  a.project_code in &#x28;SELECT project_code from project&#x29;&#xd;&#xa;and a.subject_code in &#x28;&#x27;SR1010&#x27;,&#x27;ZC1130&#x27;&#x29;--&#x6536;&#x5165;  NOP&#xd;&#xa;--and a.project_code&#x3d;&#x27;HF_HS&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa; b.zproject_code,&#xd;&#xa; b.zproject_name,&#xd;&#xa; a.subject_code,&#xd;&#xa; a.subject_name,&#xd;&#xa; year,&#xd;&#xa; month&#xd;&#xa;&#x29;&#xd;&#xa;SELECT &#x2a; from &#x28;&#xd;&#xa;SELECT &#xd;&#xa; project_code,&#xd;&#xa; project_name,&#xd;&#xa; subject_code,&#xd;&#xa; --subject_name,&#xd;&#xa; year,&#xd;&#xa; month,&#xd;&#xa;isnull&#x28;org_lj_actual,0&#x29; as org_lj_actual&#xd;&#xa;from month_actual &#xd;&#xa;&#x29; c&#xd;&#xa;PIVOT &#x28;&#xd;&#xa;sum&#x28;org_lj_actual&#x29; for &#x5b;subject_code&#x5d; in &#x28;&#x5b;SR1010&#x5d;,&#x5b;ZC1130&#x5d;&#x29; &#xd;&#xa;&#x29; T&#xd;&#xa;where  year&#x3d;year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;--where project_code&#x3d;&#x27;BJ_DJLT&#x27;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;</sql>
