    <connection>hs</connection>
    <sql>with project as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;business,&#xd;&#xa;status&#xd;&#xa;from YW_HS_PROJECT_BASEINFO&#xd;&#xa;where project_code not in &#x28;&#x27;BJ_HSH_SDDJZHT&#x27;,&#x27;SH_HSH_WJCZHT&#x27;&#x29;&#xd;&#xa;--and project_code&#x3d;&#x27;BJ_HSH_SDCFGC&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;--&#x5404;&#x8d39;&#x7528;&#x7684;&#x9879;&#x76ee;&#x6743;&#x91cd;&#xd;&#xa;,JTGLF AS &#x28;&#xd;&#xa;SELECT&#xd;&#xa;	b.project_code,&#xd;&#xa;	jysr,&#xd;&#xa;	ebitda_gop,&#xd;&#xa;  zsgl_fee,--&#x62db;&#x5546;&#x7ba1;&#x7406;&#x8d39;&#xd;&#xa;	dfl,--&#x5f97;&#x623f;&#x7387;&#xd;&#xa;  pptg_fee--&#x54c1;&#x724c;&#x63a8;&#x5e7f;&#x8d39;&#xd;&#xa;  ,sjgl_fee--&#x8bbe;&#x8ba1;&#x7ba1;&#x7406;&#x8d39;&#xd;&#xa;  ,gcgl_fee--&#x5de5;&#x7a0b;&#x7ba1;&#x7406;&#x8d39;&#xd;&#xa;FROM YY_HS_JTGL_FEE_Standard a&#xd;&#xa;INNER JOIN project  b&#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;WHERE year&#x3d;year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;--and b.project_code&#x3d;&#x27;BJ_HSH_SDCFGC&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x8fd0;&#x8425;&#x7ba1;&#x7406;&#x8d39; &#x53d6;&#x8d39;&#x91d1;&#x989d;&#x9884;&#x6d4b;&#x503c;&#x8ba1;&#x7b97;  &#x6536;&#x5165;&#x2a; &#x53d6;&#x8d39;&#x6807;&#x51c6;&#x2b;EBITDA&#x2f;GOP&#x2a; &#x53d6;&#x8d39;&#x6807;&#x51c6;&#xd;&#xa;,JYSR AS &#x28;&#xd;&#xa;	SELECT&#xd;&#xa;		project_code,&#xd;&#xa;		sum&#x28;SR_actual&#x29; as SR_actual,&#xd;&#xa;		sum&#x28;LR_actual&#x29; as LR_actual&#xd;&#xa;	FROM&#xd;&#xa;		YW_YY_BENEFIT_ANALYST_SUM&#xd;&#xa;	WHERE&#xd;&#xa;		YEAR &#x3d; YEAR &#x28;convert &#x28;varchar&#x28;7&#x29;,dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;,120&#x29; &#x2b; &#x27;-01&#x27;&#x29;&#xd;&#xa;	AND MONTH &#x3c;&#x3d; MONTH &#x28;convert &#x28;varchar&#x28;7&#x29;,dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;,120&#x29; &#x2b; &#x27;-01&#x27;&#x29;&#xd;&#xa;GROUP BY project_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from JYSR where project_code&#x3d;&#x27;BJ_HSH_SDCFGC&#x27;&#xd;&#xa; ,sr as &#x28;--7-12&#x6708;&#x6536;&#x5165;&#xd;&#xa;SELECT project_code,&#xd;&#xa;sum&#x28;jysr&#x29; as jysr&#xd;&#xa;FROM YY_HS_JTGL_FEE_ALLMONTH&#xd;&#xa;WHERE --project_CODE&#x3d;&#x27;BJ_HSH_CYCFGC&#x27;&#xd;&#xa;  left&#x28;yearmonth,4&#x29;&#x3d;convert &#x28;varchar&#x28;4&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;and yearmonth&#x3e;&#x3d;  convert &#x28;varchar&#x28;7&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;GROUP BY project_code&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;,zc as &#x28;--bpm&#x652f;&#x51fa;&#x9884;&#x7b97;&#xd;&#xa;SELECT project_code , &#xd;&#xa;SUM&#x28;ZC_budget&#x29; as ZC_budget &#xd;&#xa;FROM YW_YY_BENEFIT_ANALYST_SUM&#xd;&#xa;Where&#xd;&#xa; &#x5b;year&#x5d;&#x3d;convert &#x28;varchar&#x28;4&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;AND MONTH&#x3e;&#x3d;cast&#x28;DATENAME&#x28;month, getdate&#x28;&#x29;&#x29; as int&#x29;&#xd;&#xa;GROUP BY project_code&#xd;&#xa;&#x29; &#xd;&#xa;,sr_ebitda as &#x28;&#xd;&#xa;SELECT&#xd;&#xa;sr.project_code,&#xd;&#xa;sr.jysr&#x2b;JYSR.SR_actual as SR_actual,&#xd;&#xa;sr.jysr-zc.ZC_budget&#x2b;JYSR.LR_actual   as LR_actual&#xd;&#xa;from sr&#xd;&#xa;INNER JOIN zc&#xd;&#xa;on sr.project_code&#x3d;zc.project_code&#xd;&#xa;INNER JOIN JYSR&#xd;&#xa;on sr.project_code&#x3d;JYSR.project_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from sr_ebitda where project_code&#x3d;&#x27;BJ_HSH_SDCFGC&#x27;&#xd;&#xa;,yygl_data as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;&#x27;DSSR1.1.2&#x27; AS subject_code,&#xd;&#xa;case when b.LR_actual&#x3c;0 then A.jysr&#x2a;b.SR_actual else A.jysr&#x2a;b.SR_actual &#x2b;A.ebitda_gop&#x2a;b.LR_actual end  AS yc_data&#xd;&#xa;from  JTGLF a&#xd;&#xa;LEFT JOIN sr_ebitda b&#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT  sum&#x28;yc_data&#x29; from yygl_data--sum&#x28;yc_data&#x29; as yc_data&#xd;&#xa;--where project_code&#x3d;&#x27;BJ_HSH_SDCFGC&#x27;&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x62db;&#x5546;&#x7ba1;&#x7406;&#x8d39;&#x3d; &#x53d6;&#x8d39;&#x6807;&#x51c6;&#x2a;&#x53d6;&#x8d39;&#x57fa;&#x6570;&#x28;&#x6bcf;&#x6708;&#x65b0;&#x589e;&#x62db;&#x5546;&#x9762;&#x79ef;&#x29;&#x2f;&#x5f97;&#x623f;&#x7387;&#xd;&#xa;,zs_area_add as&#x28;&#xd;&#xa;SELECT project_code,SUM&#x28;areaadd&#x29; AS areaadd&#xd;&#xa;FROM&#x28;&#xd;&#xa;select&#xd;&#xa;	project_code,&#xd;&#xa;	SUM&#x28;zsgl_fee&#x29; AS  areaadd--&#x65b0;&#x589e;&#x62db;&#x5546;&#x9762;&#x79ef;&#xd;&#xa;from YY_HS_JTGL_FEE&#xd;&#xa;where left&#x28;yearmonth,4&#x29;&#x3d; year&#x28;GETDATE&#x28;&#x29;&#x29;&#xd;&#xa;and yearmonth &#x3c; convert&#x28;varchar&#x28;7&#x29;,GETDATE&#x28;&#x29;,120&#x29;&#xd;&#xa;GROUP BY project_code&#xd;&#xa;union all &#xd;&#xa;select&#xd;&#xa;	project_code,&#xd;&#xa;	SUM&#x28;newarea &#x29; AS areaadd --7-12&#x6708;&#x65b0;&#x589e;&#x62db;&#x5546;&#x9762;&#x79ef;&#xd;&#xa;from YY_HS_JTGL_FEE_ALLMONTH&#xd;&#xa;where left&#x28;yearmonth,4&#x29;&#x3d; year&#x28;GETDATE&#x28;&#x29;&#x29;&#xd;&#xa;and yearmonth &#x3e;&#x3d; convert&#x28;varchar&#x28;7&#x29;,GETDATE&#x28;&#x29;,120&#x29;&#xd;&#xa;GROUP BY project_code&#xd;&#xa;&#x29; A&#xd;&#xa;GROUP BY project_code&#xd;&#xa;&#x29;&#xd;&#xa;,ZSGLF_DATA AS &#x28;&#xd;&#xa;select &#xd;&#xa;       a.project_code,&#xd;&#xa;       &#x27;DSSR1.1.1&#x27; AS  subject_code,&#xd;&#xa;      -- a.yearmonth,&#xd;&#xa;       &#x28;a.areaadd &#x2a; b.zsgl_fee&#x29;&#x2f;b.dfl&#x2f;10000 as YC_data --&#x62db;&#x5546;&#x7ba1;&#x7406;&#x8d39;&#x3d; &#x53d6;&#x8d39;&#x6807;&#x51c6;&#x2a;&#x53d6;&#x8d39;&#x57fa;&#x6570;&#x28;&#x6bcf;&#x6708;&#x65b0;&#x589e;&#x62db;&#x5546;&#x9762;&#x79ef;&#x29;&#x2f;&#x5f97;&#x623f;&#x7387;      &#xd;&#xa;from zs_area_add a&#xd;&#xa;left join JTGLF b on a.project_code&#x3d;b.project_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT SUM&#x28;YC_DATA&#x29; from ZSGLF_DATA where project_code&#x3d;&#x27;BJ_HSH_CYCFGC&#x27;&#xd;&#xa;--&#x54c1;&#x724c;&#x7ba1;&#x7406;&#x8d39; &#x524d;&#x671f;&#x7ba1;&#x7406;&#x8d39;  &#x8bbe;&#x8ba1;&#x7ba1;&#x7406;&#x8d39;  &#x5de5;&#x7a0b;&#x7ba1;&#x7406;&#x8d39; &#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x4fe1;&#x606f;&#x5316;&#x7ba1;&#x7406;&#x8d39;&#x3d;&#x53d6;&#x8d39;&#x6807;&#x51c6;&#x2a;&#x53d6;&#x8d39;&#x57fa;&#x6570;&#x2f;12&#x2a;&#x6708;&#x4efd;&#x6570;   if&#x28;H4&#x3d;1,abs&#x28;round&#x28;E8 &#x2a; F8 &#x2f; 12,2&#x29;&#x2a;11-E8&#x2a;F8&#x29;,round&#x28;E8 &#x2a; F8 &#x2f; 12,2&#x29;&#x29;&#xd;&#xa;,XXHGL_data AS &#x28;&#xd;&#xa;SELECT&#xd;&#xa;	project_code,&#xd;&#xa;  &#x27;DSSR1.1.3&#x27; as subject_code,&#xd;&#xa;  --&#x27;2019-12&#x27; as yearmonth,&#xd;&#xa;	xxgl_fee as YC_data&#xd;&#xa;FROM YY_HS_JTGL_FEE_Standard&#xd;&#xa;WHERE year &#x3d; year&#x28;GETDATE&#x28;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT SUM&#x28; XXHGLF&#x29; AS YC_DATA FROM XXHGL_data&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d; &#x54c1;&#x724c;&#x63a8;&#x5e7f;&#x8d39; &#x53d6;&#x8d39;&#x57fa;&#x6570;&#xff08;&#x63a8;&#x5e7f;&#x8d39;&#x7528;&#xff09;&#x2a;&#x53d6;&#x8d39;&#x6807;&#x51c6;&#xff08;10&#x25;&#xff09;&#xd;&#xa;,PPTGF_data as &#x28;&#xd;&#xa;&#xd;&#xa;select b.project_code,   &#xd;&#xa;       &#x27;DSSR1.1.4&#x27; as subject_code,    &#xd;&#xa;       --b.year&#x2b;&#x28;case when len&#x28;b.&#x5b;month&#x5d;&#x29;&#x3d;1 then &#x27;-0&#x27;&#x2b;b.&#x5b;month&#x5d; else &#x27;-&#x27;&#x2b;b.&#x5b;month&#x5d;end&#x29; yearmonth,&#xd;&#xa;       sum&#x28;a.pptg_fee&#x2a;b.budget&#x29; as yc_data&#xd;&#xa;from YW_YY_BENEFIT_ANALYST b&#xd;&#xa;LEFT JOIN DIM_BI_YUSUAN_SUBJECT c &#xd;&#xa;ON b.subject_code&#x3d;c.bi_subject_code&#xd;&#xa;LEFT join JTGLF a&#xd;&#xa;on b.project_code&#x3d;a.project_code&#xd;&#xa;WHERE b.subject_code &#x3d;&#x27;DSGPMF4.4&#x27;&#xd;&#xa;and b.year&#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;group by b.project_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT sum&#x28;yc_data&#x29; from PPTGF_data&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x524d;&#x671f;&#x7ba1;&#x7406;&#x8d39;&#x3d;&#x6708;&#x5ea6;&#x53d6;&#x8d39;&#x91d1;&#x989d;&#x4e4b;&#x548c;&#xd;&#xa;, QQGLF_data as &#x28;&#xd;&#xa;SELECT project_code,subject_code,sum&#x28;qqgl_fee&#x29; as yc_data&#xd;&#xa;FROM &#x28;&#xd;&#xa;SELECT project_code,&#xd;&#xa;       &#x27;DSSR1.1.5&#x27; as subject_code,&#xd;&#xa;       --yearmonth,&#xd;&#xa;       sum&#x28;qqgl_fee&#x29; as qqgl_fee&#xd;&#xa;FROM YY_HS_JTGL_FEE&#xd;&#xa;where left&#x28;yearmonth,4&#x29;&#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;and yearmonth &#x3c;  convert&#x28;varchar&#x28;7&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;group by project_code&#xd;&#xa;union all&#xd;&#xa;select project_code,&#xd;&#xa;      &#x27;DSSR1.1.5&#x27; as subject_code&#xd;&#xa;    -- , yearmonth&#xd;&#xa;     , sum&#x28;qqgl_fee&#x29; as qqgl_fee&#xd;&#xa; from YY_HS_JTGL_FEE_ALLMONTH &#xd;&#xa;where left&#x28;yearmonth,4&#x29; &#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;  and yearmonth &#x3e;&#x3d; convert&#x28;varchar&#x28;7&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;group by project_code&#xd;&#xa;&#x29; a &#xd;&#xa;group by project_code,subject_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT sum&#x28;yc_data&#x29; from QQGLF_data&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;  &#x8bbe;&#x8ba1;&#x7ba1;&#x7406;&#x8d39;  &#x5de5;&#x7a0b;&#x7ba1;&#x7406;&#x8d39; &#xd;&#xa;,SJGLF_data as &#x28;&#xd;&#xa;SELECT a.project_code,A.subject_code,SUM&#x28;yc_data&#x29; AS yc_data&#xd;&#xa;FROM&#x28;&#xd;&#xa;SELECT a.project_code,&#xd;&#xa;       &#x27;DSSR1.1.6&#x27; as subject_code,&#xd;&#xa;       --a.yearmonth,&#xd;&#xa;       SUM&#x28;B.sjgl_fee&#x2a;a.sjgl_fee&#x29; as yc_data&#xd;&#xa;FROM YY_HS_JTGL_FEE a&#xd;&#xa;INNER JOIN JTGLF b &#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;where left&#x28;yearmonth,4&#x29;&#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;and yearmonth &#x3c;  convert&#x28;varchar&#x28;7&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;GROUP BY a.project_code&#xd;&#xa;union all&#xd;&#xa;select a.project_code,&#xd;&#xa;     &#x27;DSSR1.1.6&#x27; as subject_code,&#xd;&#xa;       --a.yearmonth,&#xd;&#xa;       SUM&#x28;B.sjgl_fee&#x2a;a.sjgl_fee&#x29; as yc_data&#xd;&#xa; from YY_HS_JTGL_FEE_ALLMONTH a&#xd;&#xa;INNER JOIN JTGLF b &#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;where left&#x28;yearmonth,4&#x29; &#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;  and yearmonth &#x3e;&#x3d; convert&#x28;varchar&#x28;7&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;GROUP BY a.project_code&#xd;&#xa;&#x29; A&#xd;&#xa;GROUP BY a.project_code,A.subject_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT SUM&#x28;yc_data&#x29; AS yc_data from SJGLF_data&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x5de5;&#x7a0b;&#x7ba1;&#x7406;&#x8d39;&#x3d;&#x53d6;&#x8d39;&#x57fa;&#x6570;&#xff08;&#x5de5;&#x7a0b;&#x4ea7;&#x503c;&#xff09;&#x2a;&#x53d6;&#x8d39;&#x6807;&#x51c6;&#xff08;10&#x25;&#xff09;&#xd;&#xa;,GCGLF_data as &#x28;&#xd;&#xa;SELECT a.project_code,A.subject_code,SUM&#x28;yc_data&#x29; AS yc_data FROM &#x28;&#xd;&#xa;  SELECT a.project_code,&#xd;&#xa;       &#x27;DSSR1.1.7&#x27; as subject_code,&#xd;&#xa;       --a.yearmonth,&#xd;&#xa;       sum&#x28;a.gcgl_fee&#x2a;b.gcgl_fee&#x29; as yc_data&#xd;&#xa;FROM YY_HS_JTGL_FEE a&#xd;&#xa;INNER JOIN JTGLF b &#xd;&#xa;on a.project_code&#x3d;b.project_code&#xd;&#xa;where left&#x28;yearmonth,4&#x29;&#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;and yearmonth &#x3c;  convert&#x28;varchar&#x28;7&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;group by a.project_code&#xd;&#xa;union all&#xd;&#xa;select a.project_code,&#xd;&#xa;       &#x27;DSSR1.1.7&#x27; as subject_code,&#xd;&#xa;       --a.yearmonth,&#xd;&#xa;       sum&#x28;a.gcgl_fee &#x2a; b.gcgl_fee&#x29; as yc_data&#xd;&#xa; from YY_HS_JTGL_FEE_ALLMONTH a&#xd;&#xa;INNER JOIN JTGLF b &#xd;&#xa;on a.project_code&#x3d;b.project_code &#xd;&#xa;where left&#x28;yearmonth,4&#x29; &#x3d; year&#x28;getdate&#x28;&#x29;&#x29;&#xd;&#xa;  and yearmonth &#x3e;&#x3d; convert&#x28;varchar&#x28;7&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;GROUP BY a.project_code&#xd;&#xa;&#x29; A&#xd;&#xa;GROUP BY a.project_code,A.subject_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT SUM&#x28;YC_DATA&#x29; FROM GCGLF_data&#xd;&#xa;,data as &#x28;&#xd;&#xa;SELECT &#x2a; from ZSGLF_DATA&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#x2a; from yygl_data&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#x2a; from XXHGL_data&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#x2a; from PPTGF_data&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#x2a; from QQGLF_data&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#x2a; from SJGLF_data&#xd;&#xa;union ALL&#xd;&#xa;SELECT &#x2a; from GCGLF_data&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; FROM data&#xd;&#xa;, subject1 as&#x28;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x79d1;&#x76ee;&#x5168;&#x8def;&#x5f84;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;select&#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.f_code,&#xd;&#xa;a.subject_level as level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;CAST&#x28;a.subject_code&#x2b;&#x27;,&#x27; AS VARCHAR&#x28;max&#x29;&#x29; AS fullpath&#xd;&#xa;from YW_HS_DIM_PROFIT_SUBJECT_SGGS a&#xd;&#xa;where len&#x28;a.f_code&#x29; &#x3d; 0&#xd;&#xa;and a.ndmark&#x3d;&#x27;2019&#x27;&#xd;&#xa;union all&#xd;&#xa;select&#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.f_code,&#xd;&#xa;a.subject_level as level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;cast&#x28;subject1.fullpath &#x2b; &#x27;-&#x3e;&#x27;&#x2b; a.subject_code&#x2b;&#x27;,&#x27;  as VARCHAR&#x28;max&#x29;&#x29;as fullpath&#xd;&#xa;from YW_HS_DIM_PROFIT_SUBJECT_SGGS a&#xd;&#xa;inner join subject1  on subject1.subject_code &#x3d; a.f_code&#xd;&#xa;where  a.ndmark&#x3d;&#x27;2019&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;,hz_data as &#x28;&#xd;&#xa;SELECT&#xd;&#xa;a.project_code, &#xd;&#xa;a.subject_code,&#xd;&#xa;b.fullpath,&#xd;&#xa;--a.yearmonth,&#xd;&#xa;sum&#x28;isnull&#x28;YC_data,0&#x29;&#x29; as sum_ycdata&#xd;&#xa;from data  a&#xd;&#xa;INNER JOIN  subject1 b&#xd;&#xa;on a.subject_code&#x3d;b.subject_code&#xd;&#xa;group by a.project_code,  a.subject_code,b.fullpath&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from subject1&#xd;&#xa;SELECT&#xd;&#xa;a.project_code,&#xd;&#xa;b.subject_code,&#xd;&#xa;&#x27;2019-12&#x27; AS yearmonth,&#xd;&#xa;sum&#x28;a.sum_ycdata&#x29; as YC_data&#xd;&#xa;from hz_data a&#xd;&#xa;INNER JOIN  subject1 b&#xd;&#xa;on charindex&#x28;b.fullpath,a.fullpath&#x29;&#x3d;1&#xd;&#xa;group by b.subject_code,a.project_code&#xd;&#xa;ORDER BY b.subject_code</sql>
