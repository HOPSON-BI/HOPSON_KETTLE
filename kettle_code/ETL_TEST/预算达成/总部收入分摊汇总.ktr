    <connection>HS</connection>
    <sql>WITH JYSR AS ---各项目收入、累计收入
(
SELECT project_code,
	  year+(case when len(month)=1 then '-0'+month else '-'+month end) yearmonth,
		year,
		month,
	  SR_actual,
	  LR_actual,
		sum(lr_actual) over (partition by project_code,year order by project_code,year,cast(month as int)) as lj_actual
FROM YW_YY_BENEFIT_ANALYST_SUM
where year>=2019
group by project_code,year,month,sr_actual,lr_actual
),
jtglf_standard AS ---集团管理费标准
(
SELECT project_code,
       zsgl_fee/(10000*dfl) zsgl_fee,
       jysr,
       ebitda_gop,
       xxgl_fee,
       pptg_fee,
       sjgl_fee,
       gcgl_fee,
			 [year]
FROM YY_HS_JTGL_FEE_Standard
where 
 project_code not in('BJ_HSH_SDDJZHT','SH_HSH_WJCZHT','GZ_HSH_JH_ZHT')
),
jtglf as ---集团管理费取费基数
(
select
	project_code,
	yearmonth,
	zsgl_fee,
	qqgl_fee,
	sjgl_fee,
	gcgl_fee
from YY_HS_JTGL_FEE
),
pptgf as ---品牌推广费从预算达成页面取数
(
select b.project_code,       
       b.year+(case when len(b.[month])=1 then '-0'+b.[month] else '-'+b.[month]end) yearmonth,
       b.budget pptg_fee
from YW_YY_BENEFIT_ANALYST b
LEFT JOIN DIM_BI_YUSUAN_SUBJECT c ON b.subject_code=c.bi_subject_code
WHERE b.subject_code ='DSGPMF4.4'
and b.project_code not in ('BJ_HSH_SDDJZHT','SH_HSH_WJCZHT','GZ_HSH_JH_ZHT')
--group by b.year+(case when len(b.[month])=1 then '-0'+b.[month] else '-'+b.[month]end)

)

--除运营管理费外其他的管理收入计算
,month_HZ as(
SELECT
     a.project_code,
	 c.yearmonth,
	isnull(b.zsgl_fee, 0) * isnull(c.zsgl_fee, 0) ZSGLF,
	--sum(isnull(b.jysr, 0) * isnull(d.SR_actual, 0)) + case when sum(isnull(b.ebitda_gop, 0) * isnull(d.lj_actual, 0)) &lt;0 then 0 else sum(isnull(b.ebitda_gop, 0) * isnull(d.LR_actual, 0)) end  as YYGLF,
	--sum(b.xxgl_fee)  as XXHGLF,
	case when right(c.yearmonth,2) = '01' then b.xxgl_fee-round(b.xxgl_fee/12,2)*11 else  round(b.xxgl_fee/12,2) end as  XXHGLF,
	f.pptg_fee*b.pptg_fee PPTGF,
    c.qqgl_fee QQGLF,
	b.sjgl_fee*c.sjgl_fee SJGLF,
	b.gcgl_fee*c.gcgl_fee GCGLF
FROM
	YW_HS_PROJECT_BASEINFO a
LEFT JOIN jtglf c ON a.project_code = c.project_code 
LEFT JOIN jtglf_standard b ON a.project_code = b.project_code and b.year = left(c.yearmonth,4)
LEFT JOIN JYSR d ON a.project_code = d.project_code and c.yearmonth=d.yearmonth
left join pptgf f on a.project_code = f.project_code and c.yearmonth=f.yearmonth
where --left(c.yearmonth,4)=left('2019-03',4) and
--and right(c.yearmonth,2)&lt;=month('${yearmonth}'+'-01') and
 a.project_code not in('BJ_HSH_SDDJZHT','SH_HSH_WJCZHT','GZ_HSH_JH_ZHT')
),
----运营管理费算累计值
yyglf as(
select
a.project_code,
d.year,
d.month,
sum(isnull(b.jysr, 0) * isnull(d.SR_actual, 0)) over (partition by a.project_code,d.year order by a.project_code,d.year,cast(d.month as int)) + case when sum(isnull(b.ebitda_gop, 0) * isnull(d.lj_actual, 0)) &lt;=0 then 0 else sum(isnull(b.ebitda_gop, 0) * isnull(d.LR_actual, 0)) over (partition by a.project_code,d.year order by a.project_code,d.year,cast(d.month as int)) end  as YYGLF
from YW_HS_PROJECT_BASEINFO a
LEFT JOIN JYSR d ON a.project_code = d.project_code
LEFT JOIN jtglf_standard b ON a.project_code = b.project_code and b.year = d.year
group by a.project_code,d.yearmonth,b.jysr,d.sr_actual,b.ebitda_gop,d.lj_actual,d.lr_actual,d.year,d.month
),

---运营管理费做累计倒减
yyglf_hz as(
select a.project_code,a.year+(case when len(a.month)=1 then '-0'+a.month else '-'+a.month end) yearmonth
,sum(a.yyglf)-sum(isnull(b.yyglf,0)) as yyglf 
from yyglf a 
left join yyglf b on a.project_code = b.project_code and a.year = b.year and a.month = b.month+1 
where a.year is not null 
group by a.project_code,a.year,a.month)

SELECT
a.yearmonth,
'DSSR1.1.2' as subject_code,
'运营管理费' as subject_name
--,a.project_code
--,a.yyglf*c.ratio as je
,sum(a.yyglf*c.ratio) as je
,c.body_code
FROM
yyglf_hz a
LEFT JOIN yw_hs_project_zb b on a.project_code = b.project_code
LEFT JOIN YW_HS_ZB_CONFIG c on b.shiyebu = c.shiyebu and b.quyu = c.quyu and c.subject_code = 'DSSR1.1.2'
where a.yyglf &lt;&gt; 0 
and  left(a.yearmonth,4) >= '2019' --and c.body_code = 'ZB10011401'
GROUP BY a.yearmonth,c.body_code

UNION
--确认没有问题
SELECT
a.yearmonth,
'DSSR1.1.1' as subject_code,
'招商管理费' as subject_name,
--a.project_code,
sum(a.zsglf*c.ratio) as je,
c.body_code
FROM
month_hz a
LEFT JOIN yw_hs_project_zb b on a.project_code = b.project_code
LEFT JOIN YW_HS_ZB_CONFIG c on b.shiyebu = c.shiyebu and b.quyu = c.quyu and c.subject_code = 'DSSR1.1.1'
where a.zsglf &lt;&gt; 0 
and  left(a.yearmonth,4) >= '2019' --and c.body_code = 'ZB10011401'
GROUP BY a.yearmonth,c.body_code

UNION
--信息化管理费确认无误
SELECT
a.yearmonth,
'DSSR1.1.3' as subject_code,
'信息化管理费' as subject_name,
--a.project_code,
sum(a.xxhglf*c.ratio) as je,
c.body_code
FROM
month_hz a
LEFT JOIN yw_hs_project_zb b on a.project_code = b.project_code
LEFT JOIN YW_HS_ZB_CONFIG c on b.shiyebu = c.shiyebu and b.quyu = c.quyu and c.subject_code = 'DSSR1.1.3'
where a.xxhglf &lt;&gt; 0 
and  left(a.yearmonth,4) >= '2019' --and c.body_code = 'ZB10011401'
GROUP BY a.yearmonth,c.body_code

UNION
--品牌推广费确认无误
SELECT
a.yearmonth,
'DSSR1.1.4' as subject_code,
'品牌推广费' as subject_name,
--a.project_code,
sum(a.PPTGF*c.ratio) as je,
c.body_code
FROM
month_hz a
LEFT JOIN yw_hs_project_zb b on a.project_code = b.project_code
LEFT JOIN YW_HS_ZB_CONFIG c on b.shiyebu = c.shiyebu and b.quyu = c.quyu and c.subject_code = 'DSSR1.1.4'
where a.PPTGF &lt;&gt; 0 
and  left(a.yearmonth,4) >= '2019' --and c.body_code = 'ZB10011401'
GROUP BY a.yearmonth,c.body_code

UNION
--前期管理费确认无误
SELECT
a.yearmonth,
'DSSR1.1.5' as subject_code,
'前期管理费' as subject_name,
--a.project_code,
sum(a.QQGLF*c.ratio) as je,
c.body_code
FROM
month_hz a
LEFT JOIN yw_hs_project_zb b on a.project_code = b.project_code
LEFT JOIN YW_HS_ZB_CONFIG c on b.shiyebu = c.shiyebu and b.quyu = c.quyu and c.subject_code = 'DSSR1.1.5'
where a.QQGLF &lt;&gt; 0 
and  left(a.yearmonth,4) >= '2019' --and c.body_code = 'ZB10011401'
GROUP BY a.yearmonth,c.body_code

UNION
--设计管理费确认无误
SELECT
a.yearmonth,
'DSSR1.1.6' as subject_code,
'设计管理费' as subject_name,
--a.project_code,
sum(a.SJGLF) as je,
'ZB10011410' as body_code
FROM
month_hz a
where a.SJGLF &lt;&gt; 0 
