    <sql>with km as &#x28;&#xd;&#xa;&#xd;&#xa;--&#x65b0;&#x52a0;&#x7684;&#x8d22;&#x52a1;&#x6570;&#x636e;&#xd;&#xa;SELECT&#xd;&#xa;--	YEAR &#x2b;&#x28;CASE WHEN len&#x28;MONTH&#x29; &#x3d; 1 THEN &#x27;-0&#x27; &#x2b; MONTH ELSE &#x27;-&#x27; &#x2b; MONTH END&#x29; yearmonth,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;	&#x5b;value&#x5d; as actual,&#xd;&#xa;	subject_code&#xd;&#xa;FROM&#xd;&#xa;	YW_YY_PROFIT_ZB_RESULT&#xd;&#xa;where &#x28;subject_code in &#x28;&#x27;DSYZ2.3.4&#x27;,&#x27;DSYZ2.2.18&#x27;,&#x27;DSYZ2.4.19&#x27;,&#x27;DSYZ2.4.20&#x27;,&#x27;DSYZ2.4.21&#x27;,&#x27;DSYZ2.6.1&#x27;,&#x27;DSYZ2.6.2&#x27;,&#x27;DSFZ4&#x27;,&#x27;DSCW6.1&#x27;,&#x27;DSCW6.2&#x27;,&#x27;DSCW6.3&#x27;,&#x27;DSCW6.4&#x27;&#x29; OR SUBJECT_CODE LIKE &#x27;DSYZ2.5&#x25;&#x27;&#x29;&#xd;&#xa;AND PROJECT_CODE &#x3d; &#x27;SG_GS&#x27;&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;--CONVERT&#x28;varchar&#x28;20&#x29;,YEAR&#x2b;&#x27;-&#x27;&#x2b;MONTH&#x2b;&#x27;-1&#x27;,120&#x29;,&#xd;&#xa;--case when len&#x28;month&#x29; &#x3d; 1 then year&#x2b;&#x27;-0&#x27;&#x2b;month else year&#x2b;&#x27;-&#x27;&#x2b;month end as yearmonth,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;SUM&#x28;ACTUAL&#x29;&#x2a;10000 as actual,&#xd;&#xa;&#x27;DSSR1.2.3&#x27; AS subject_code&#xd;&#xa;from YW_YY_BENEFIT_ANALYST_XLS&#xd;&#xa;where subject_code in &#x28;&#x27;YFKM12&#x27;,&#x27;KSKM12&#x27;&#x29;&#xd;&#xa;group by year,month&#xd;&#xa;&#xd;&#xa;&#x29;&#xd;&#xa;, km1 as &#x28;&#xd;&#xa;select  ----&#x5176;&#x4ed6;&#x7ba1;&#x7406;&#x8d39;&#x7528;&#x6309;&#x6bd4;&#x4f8b;&#x5206;&#x644a;&#xd;&#xa;&#x27;HS_SG&#x27; as project_code,&#xd;&#xa;A.YEAR,&#xd;&#xa;a.month,&#xd;&#xa;b.department_id as body_code,&#xd;&#xa;d.body_name,&#xd;&#xa;A.SUBJECT_code,&#xd;&#xa;c.bi_subject_name as subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;a.actual&#x2a;b.ratio as buhanshuijia,&#xd;&#xa;&#x27;BI&#x27; as src,&#xd;&#xa;GETDATE&#x28;&#x29; createdate,&#xd;&#xa;&#x27;bi&#x27; createperson&#xd;&#xa;from km a &#xd;&#xa;full join YW_YY_RATIO_SYB b on a.year &#x3d; b.year&#xd;&#xa;left join DIM_BPM_BI_SUBJECT_ZB c on a.subject_code &#x3d; c.bi_subject_code&#xd;&#xa;left join YW_HS_ZB_BODY_MAP d on B.department_id &#x3d; d.body_code&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select ----BPM&#x6570;&#x636e;&#x53d6;&#x6570;&#xd;&#xa;&#x27;HS_SG&#x27; as project_code,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;A.body_code,&#xd;&#xa;c.body_name,&#xd;&#xa;B.bi_subject_code AS SUBJECT_CODE,&#xd;&#xa;b.bi_subject_name as subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;--case when len&#x28;a.month&#x29; &#x3d; 1 then a.year&#x2b;&#x27;-0&#x27;&#x2b;a.month else a.year&#x2b;&#x27;-&#x27;&#x2b;a.month end as yearmonth,&#xd;&#xa;A.actual as buhanshuijia,&#xd;&#xa;&#x27;BPM&#x27; as src,&#xd;&#xa;GETDATE&#x28;&#x29; createdate,&#xd;&#xa;		&#x27;bi&#x27; createperson&#xd;&#xa;from ODS_BPM_ACTUAL_DETAIL a&#xd;&#xa;left join DIM_BPM_BI_SUBJECT_ZB b on a.subject_code &#x3d; b.bpm_subject_code&#xd;&#xa;left join YW_HS_ZB_BODY_MAP c on a.body_code &#x3d; c.body_code&#xd;&#xa;--left join YW_BI_BPM_PROJECT_MAP c on a.project_code &#x3d; c.bpm_project_code&#xd;&#xa;where &#x28;&#x28;b.bi_subject_code like &#x27;DSYZ2&#x25;&#x27;  AND B.bi_subject_code NOT LIKE &#x27;DSYZ2.6&#x25;&#x27; and b.bi_subject_code not like &#x27;DSYZ2.5&#x25;&#x27; AND B.bi_subject_code NOT IN &#x28;&#x27;DSYZ2.2.18&#x27;,&#x27;DSYZ2.4.19&#x27;,&#x27;DSYZ2.4.20&#x27;,&#x27;DSYZ2.4.21&#x27;,&#x27;DSYZ2.3.4&#x27;&#x29;&#x29; or b.bi_subject_code in &#x28;&#x27;DSFZ5&#x27;&#x29;&#x29;&#xd;&#xa;AND A.projecttype &#x3d; 5 &#xd;&#xa;AND A.project_code &#x3d; &#x27;HSHS412&#x27;&#xd;&#xa;AND A.body_code &#x3c;&#x3e; &#x27;ZB100101&#x27;&#xd;&#xa;--AND C.bi_project_code &#x3d; &#x27;SGZB&#x27; &#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa; &#xd;&#xa;select ---&#x8d22;&#x52a1;&#x6295;&#x8d44;&#x6536;&#x5165; &#x653e;&#x4e2d;&#x53f0;&#xd;&#xa;&#x27;HS_SG&#x27;  project_code,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;&#x27;ZB10011415&#x27; body_code,&#xd;&#xa;&#x27;&#x4e2d;&#x53f0;&#x27; body_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;0,&#xd;&#xa;a.value,&#xd;&#xa;&#x27;CW&#x27;  src,&#xd;&#xa;GETDATE&#x28;&#x29; createdate,&#xd;&#xa;		&#x27;bi&#x27; createperson&#xd;&#xa;FROM YW_YY_PROFIT_ZB_RESULT a&#xd;&#xa;WHERE a.subject_code IN &#x28;&#x27;DSSR1.3.1&#x27;,&#x27;DSSR1.3.2&#x27;&#x29; and a.project_code &#x3d; &#x27;SG_GS&#x27;&#xd;&#xa;&#xd;&#xa;UNION ALL&#xd;&#xa;&#xd;&#xa;SELECT&#xd;&#xa;&#x27;HS_SG&#x27; as project_code,&#xd;&#xa;year&#x28;a.yearmonth&#x2b;&#x27;-01&#x27;&#x29; as &#x5b;year&#x5d;,&#xd;&#xa;month&#x28;a.yearmonth&#x2b;&#x27;-01&#x27;&#x29; as &#x5b;month&#x5d;,&#xd;&#xa;b.body_code,&#xd;&#xa;b.body_name,&#xd;&#xa;a.SUBJECT_CODE,&#xd;&#xa;a.subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;--case when len&#x28;a.month&#x29; &#x3d; 1 then a.year&#x2b;&#x27;-0&#x27;&#x2b;a.month else a.year&#x2b;&#x27;-&#x27;&#x2b;a.month end as yearmonth,&#xd;&#xa;a.buhanshuijia&#x2a;10000 as buhanshuijia,&#xd;&#xa;&#x27;BI&#x27; as src,&#xd;&#xa;GETDATE&#x28;&#x29; createdate,&#xd;&#xa;		&#x27;bi&#x27; createperson&#xd;&#xa;FROM yw_hs_zb_income a&#xd;&#xa;LEFT JOIN YW_HS_ZB_BODY_MAP b on a.bodycode &#x3d; b.BODY_CODE&#xd;&#xa;&#x29;&#xd;&#xa;--select &#x2a; from km1&#xd;&#xa;, km2 as &#x28; ---&#x81ea;&#x8425;&#x6536;&#x5165;&#x3001;&#x8d22;&#x52a1;&#x8d39;&#x7528;&#x6c47;&#x603b;&#xd;&#xa;select&#xd;&#xa;&#x2a;&#xd;&#xa;from km1&#xd;&#xa;where subject_code not in &#x28;&#x27;DSSR1.1&#x27;,&#x27;DSSR1.2&#x27;,&#x27;DSSR1.3&#x27;,&#x27;DSCW6&#x27;&#x29;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;a.project_code,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.body_code,&#xd;&#xa;a.body_name,&#xd;&#xa;b.f_id as subject_code,&#xd;&#xa;c.BI_subject_name as subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;sum&#x28;a.buhanshuijia&#x29; as buhanshuijia,&#xd;&#xa;&#x27;BI&#x27; AS src,&#xd;&#xa;a.createdate,&#xd;&#xa;a.createperson&#xd;&#xa;from km1 a&#xd;&#xa;left join DIM_BPM_BI_SUBJECT_ZB b on a.subject_code &#x3d; b.BI_subject_code&#xd;&#xa;left join DIM_BPM_BI_SUBJECT_ZB c on b.f_id &#x3d; c.bi_subject_code&#xd;&#xa;where b.f_id in &#x28;&#x27;DSSR1.1&#x27;,&#x27;DSSR1.2&#x27;,&#x27;DSSR1.3&#x27;,&#x27;DSCW6&#x27;&#x29;&#xd;&#xa;GROUP BY a.project_code,a.year,a.month,a.body_code,a.body_name,b.f_id,c.BI_subject_name,a.createdate,a.createperson&#xd;&#xa;&#x29;&#xd;&#xa;,srhz as &#x28; ---&#x6536;&#x5165;&#x6c47;&#x603b;&#xd;&#xa;select&#xd;&#xa;project_code,&#xd;&#xa;year,&#xd;&#xa;month,&#xd;&#xa;body_code,&#xd;&#xa;body_name,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name,&#xd;&#xa;actual,&#xd;&#xa;buhanshuijia&#xd;&#xa;from km2&#xd;&#xa;where &#xd;&#xa;subject_code not in &#x28;&#x27;DSSR1&#x27;&#x29;&#xd;&#xa;and year &#x3d; year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29; &#xd;&#xa;--and month &#x3d; month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#xd;&#xa;UNION ALL&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;a.project_code,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.body_code,&#xd;&#xa;a.body_name,&#xd;&#xa;b.f_id as subject_code,&#xd;&#xa;c.BI_subject_name as subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;sum&#x28;a.buhanshuijia&#x29; as buhanshuijia&#xd;&#xa;from km2 a&#xd;&#xa;left join DIM_BPM_BI_SUBJECT_ZB b on a.subject_code &#x3d; b.bi_subject_code&#xd;&#xa;left join DIM_BPM_BI_SUBJECT_ZB c on b.f_id &#x3d; c.bi_subject_code&#xd;&#xa;where b.f_id in &#x28;&#x27;DSSR1&#x27;&#x29;&#xd;&#xa;and a.year &#x3d; year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29; &#xd;&#xa;--and a.month &#x3d; month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;GROUP BY a.project_code,a.year,a.month,a.body_code,a.body_name,b.f_id,c.BI_subject_name&#xd;&#xa;&#x2f;&#x2a;&#xd;&#xa;UNION all&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;a.project_code,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.body_code,&#xd;&#xa;a.body_name,&#xd;&#xa;&#x27;DSYZ2.5.1.2&#x27; as subject_code,&#xd;&#xa;&#x27;&#x57ce;&#x5e02;&#x7ef4;&#x62a4;&#x5efa;&#x8bbe;&#x8d39;&#x27; as subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;sum&#x28;a.buhanshuijia&#x29;&#x2a;0.06&#x2a;0.07 as buhanshuijia&#xd;&#xa;from km2 a&#xd;&#xa;where a.subject_code in &#x28;&#x27;DSSR1.1&#x27;,&#x27;DSSR1.3&#x27;&#x29;&#xd;&#xa;and a.year &#x3d; year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29; &#xd;&#xa;--and a.month &#x3d; month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;GROUP BY a.project_code,a.year,a.month,a.body_code,a.body_name&#xd;&#xa;&#xd;&#xa;UNION all&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;a.project_code,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.body_code,&#xd;&#xa;a.body_name,&#xd;&#xa;&#x27;DSYZ2.5.1.3&#x27; as subject_code,&#xd;&#xa;&#x27;&#x6559;&#x80b2;&#x8d39;&#x53ca;&#x9644;&#x52a0;&#x27; as subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;sum&#x28;a.buhanshuijia&#x29;&#x2a;0.06&#x2a;0.03 as buhanshuijia&#xd;&#xa;from km2 a&#xd;&#xa;where a.subject_code in &#x28;&#x27;DSSR1.1&#x27;,&#x27;DSSR1.3&#x27;&#x29; &#xd;&#xa;and a.year &#x3d; year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29; &#xd;&#xa;--and a.month &#x3d; month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;GROUP BY a.project_code,a.year,a.month,a.body_code,a.body_name&#xd;&#xa;&#xd;&#xa;UNION all&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;a.project_code,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.body_code,&#xd;&#xa;a.body_name,&#xd;&#xa;&#x27;DSYZ2.5.1.4&#x27; as subject_code,&#xd;&#xa;&#x27;&#x5730;&#x65b9;&#x6559;&#x80b2;&#x8d39;&#x9644;&#x52a0;&#x27; as subject_name,&#xd;&#xa;0 as actual,&#xd;&#xa;sum&#x28;a.buhanshuijia&#x29;&#x2a;0.06&#x2a;0.02 as buhanshuijia&#xd;&#xa;from km2 a&#xd;&#xa;where a.subject_code in &#x28;&#x27;DSSR1.1&#x27;,&#x27;DSSR1.3&#x27;&#x29;&#xd;&#xa;and a.year &#x3d; year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29; &#xd;&#xa;--and a.month &#x3d; month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;GROUP BY a.project_code,a.year,a.month,a.body_code,a.body_name&#xd;&#xa;&#x2a;&#x2f; &#xd;&#xa;---&#x652f;&#x51fa;&#x4e0b;&#x9762;&#x7684;&#x7a0e;&#x8d39;&#x5168;&#x4ece;&#x8d22;&#x52a1;&#x53d6;&#xd;&#xa;&#x29;&#xd;&#xa;,kmlj as&#x28;&#xd;&#xa;select&#xd;&#xa;a.bi_subject_code as subject_code,&#xd;&#xa;a.f_id,&#xd;&#xa;CAST&#x28;a.bi_subject_code AS VARCHAR&#x28;100&#x29;&#x29; AS fullname&#xd;&#xa;from DIM_BPM_BI_SUBJECT_ZB a&#xd;&#xa;where len&#x28;a.f_id&#x29; &#x3d; 0&#xd;&#xa;&#xd;&#xa;union all&#xd;&#xa;&#xd;&#xa;select&#xd;&#xa;a.bi_subject_code as subject_code,&#xd;&#xa;a.f_id,&#xd;&#xa;cast&#x28;kmlj.fullname &#x2b; &#x27;-&#x3e;&#x27;&#x2b; a.bi_subject_code as VARCHAR&#x28;100&#x29;&#x29; as fullname&#xd;&#xa;from DIM_BPM_BI_SUBJECT_ZB a&#xd;&#xa;inner join kmlj on kmlj.subject_code &#x3d; a.f_id&#xd;&#xa;&#x29;&#xd;&#xa;,sjje as &#x28;&#xd;&#xa;select&#xd;&#xa;a.project_code,&#xd;&#xa;a.body_code,&#xd;&#xa;a.body_name,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.actual,&#xd;&#xa;a.buhanshuijia,&#xd;&#xa;b.subject_code,&#xd;&#xa;b.fullname&#xd;&#xa;from kmlj b&#xd;&#xa;inner join srhz a on a.subject_code &#x3d; b.subject_code&#xd;&#xa;left join DIM_BPM_BI_SUBJECT_ZB c on a.subject_code &#x3d; c.bi_subject_code&#xd;&#xa;where c.is_moji &#x3d; 1  &#xd;&#xa;and a.year &#x3d; year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29; &#xd;&#xa;--and a.month &#x3d; month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29; &#xd;&#xa;&#x29;&#xd;&#xa;,je as &#x28;&#xd;&#xa;select&#xd;&#xa;c.project_code,&#xd;&#xa;c.body_code,&#xd;&#xa;c.body_name,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;sum&#x28;isnull&#x28;c.actual,0&#x29;&#x29; as actual,&#xd;&#xa;sum&#x28;isnull&#x28;c.buhanshuijia,0&#x29;&#x29; as buhanshuijia,&#xd;&#xa;b.subject_code,&#xd;&#xa;sum&#x28;isnull&#x28;c.buhanshuijia,0&#x29;&#x29; as pre_actual&#xd;&#xa;from kmlj b&#xd;&#xa;inner join sjje c on charindex&#x28;b.fullname&#x2b;&#x27;-&#x3e;&#x27;,c.fullname&#x29;&#x3d;1&#xd;&#xa;group