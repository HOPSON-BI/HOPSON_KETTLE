    <sql>--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;EAS&#x603b;&#x90e8;&#x6570;&#x636e;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;&#xd;&#xa;with subject1 as&#x28;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x79d1;&#x76ee;&#x5168;&#x8def;&#x5f84;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;select&#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.f_code,&#xd;&#xa;a.subject_level as level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;CAST&#x28;a.subject_code&#x2b;&#x27;,&#x27; AS VARCHAR&#x28;max&#x29;&#x29; AS fullpath&#xd;&#xa;from YW_HS_DIM_PROFIT_SUBJECT_SGGS a&#xd;&#xa;where len&#x28;a.f_code&#x29; &#x3d; 0&#xd;&#xa;and a.ndmark&#x3d;&#x27;2019&#x27;&#xd;&#xa;union all&#xd;&#xa;select&#xd;&#xa;a.subject_code as subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.f_code,&#xd;&#xa;a.subject_level as level,&#xd;&#xa;a.is_last,&#xd;&#xa;a.subject_px,&#xd;&#xa;cast&#x28;subject1.fullpath &#x2b; &#x27;-&#x3e;&#x27;&#x2b; a.subject_code&#x2b;&#x27;,&#x27;  as VARCHAR&#x28;max&#x29;&#x29;as fullpath&#xd;&#xa;from YW_HS_DIM_PROFIT_SUBJECT_SGGS a&#xd;&#xa;inner join subject1  on subject1.subject_code &#x3d; a.f_code&#xd;&#xa;where  a.ndmark&#x3d;&#x27;2019&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from subject1 where subject_code&#x3d;&#x27;DSCE17.1.4&#x27;&#xd;&#xa;,project as &#x28;--&#x9879;&#x76ee;&#xd;&#xa;SELECT &#x2a; from YW_HS_SG_BI_MAP where depth&#x3d;&#x27;3&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;,shijian as &#x28;--&#x65f6;&#x95f4;&#x5168;&#x5339;&#x914d;&#xd;&#xa;SELECT DISTINCT year, month from YW_YY_EAS_ACCOUNTBALANCE where year&#x3e;&#x3d;&#x27;2019&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;,subject as &#x28;&#xd;&#xa;SELECT &#x2a; from subject1 c&#xd;&#xa;  --where project_code in &#x28;&#x27;KSD_SH&#x27;,&#x27;YFFK&#x27;&#x29;--ORDER BY project_code,project_px&#xd;&#xa;&#x29;&#xd;&#xa;,project_shijian as &#x28;&#xd;&#xa;SELECT&#xd;&#xa;a.&#x2a;,b.&#x2a;&#xd;&#xa;from project a&#xd;&#xa;LEFT JOIN shijian b&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;--SELECT &#x2a; from subject where subject_code&#x3d;&#x27;DSYZ2.4.19&#x27;&#xd;&#xa;,eas_km_sj as &#x28;--eas&#x672b;&#x7ea7;&#x4f1a;&#x8ba1;&#x79d1;&#x76ee;&#xd;&#xa;select &#xd;&#xa; a.project_code,&#xd;&#xa;	a.project_name,&#xd;&#xa;	a.subject_code,&#xd;&#xa;	a.subject_name,&#xd;&#xa;	ISNULL&#x28;a.assistprojectnum,1&#x29; AS assistprojectnum,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;code&#xd;&#xa;	ISNULL&#x28;a.assistprojecttype,1&#x29; AS assistprojecttype,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.assistprojectname,1&#x29; AS assistprojectname,   --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7684;&#x540d;&#x79f0;&#xd;&#xa;	a.year,&#xd;&#xa;	a.month,&#xd;&#xa;	case when CHARINDEX&#x28;&#x27;1&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_J &#xd;&#xa;     when CHARINDEX&#x28;&#x27;2&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_D  &#xd;&#xa;     ELSE a.period_fs end as sj&#xd;&#xa;from YW_YY_EAS_ACCOUNTBALANCE a&#xd;&#xa;WHERE project_code IN &#x28;SELECT DISTINCT pk_corp as project_code FROM project&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from km_sj where project_code&#x3d;&#x27;SG_HS_BJ&#x27;and subject_code in  &#x28;&#x27;6601.04.33&#x27;,&#x27;6603.01&#x27;,&#x27;6603.06&#x27;&#x29;&#xd;&#xa;,eas_km_last as&#x28;&#xd;&#xa;SELECT &#xd;&#xa;c.pk_corp as project_code,&#xd;&#xa;c.name as project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa; c.year,&#xd;&#xa;	c.month,&#xd;&#xa;	sum&#x28;a.fx&#x2a; isnull&#x28;b.sj,0&#x29;&#x29; as  sj  --&#x672c;&#x671f;&#x53d1;&#x751f;  &#x501f;&#x65b9;&#xd;&#xa;&#xd;&#xa;from YW_HS_DIM_SUBJECT_KJKM_ZB a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN eas_km_sj b &#xd;&#xa;on a.kj_code&#x3d;b.subject_code&#xd;&#xa;and a.assistprojecttype&#x3d;b.assistprojecttype&#xd;&#xa;and a.assistprojectname&#x3d;b.assistprojectname&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;and c.pk_corp&#x3d;b.project_code&#xd;&#xa;where a.source&#x3d;&#x27;km&#x27;&#xd;&#xa;and  c.PK_FATHERMEMBER&#x3d;&#x27;1001001&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c.pk_corp,&#xd;&#xa;c.name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--b.subject_code as kj_code,&#xd;&#xa;c.year,&#xd;&#xa;c.month&#xd;&#xa;&#xd;&#xa;&#x29; &#xd;&#xa;--SELECT &#x2a;from eas_km_last where  year&#x3d;&#x27;2019&#x27; and month&#x3d;&#x27;4&#x27; and kj_code&#x3d;&#x27;6117&#x27;&#xd;&#xa;,eas_km_last_ok as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;b.fullpath,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.sj&#xd;&#xa;from eas_km_last a&#xd;&#xa;INNER JOIN subject b &#xd;&#xa;on a.subject_code&#x3d;b.subject_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from km_last_ok  where project_code&#x3d;&#x27;SG_HS_BJ&#x27; and subject_code&#x3d;&#x27;DSYZ2.4.19&#x27;&#xd;&#xa;,eas_jbsj as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;c. pk_corp as project_code,&#xd;&#xa;c.name as project_name,&#xd;&#xa;a.level,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;b.sj&#xd;&#xa;from subject a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN eas_km_last_ok b&#xd;&#xa;on charindex&#x28;a.fullpath,b.fullpath&#x29;&#x3d;1&#xd;&#xa;and c.pk_corp&#x3d;b.project_code&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;where   c.PK_FATHERMEMBER&#x3d;&#x27;1001001&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from eas_jbsj where year&#x3d;&#x27;2019&#x27; and month&#x3d;&#x27;4&#x27; and subject_code&#x3d;&#x27;DSSR1.1.2&#x27;&#xd;&#xa;--&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;NC&#x603b;&#x90e8;&#x6570;&#x636e;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#x3d;&#xd;&#xa;,nc_km_sj as &#x28;--eas&#x672b;&#x7ea7;&#x4f1a;&#x8ba1;&#x79d1;&#x76ee;&#xd;&#xa;select &#xd;&#xa; a.project_code,&#xd;&#xa;	a.project_name,&#xd;&#xa;	a.subject_code,&#xd;&#xa;	a.subject_name,&#xd;&#xa;	ISNULL&#x28;a.assistprojectnum,1&#x29; AS assistprojectnum,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;code&#xd;&#xa;	ISNULL&#x28;a.assistprojecttype,1&#x29; AS assistprojecttype,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.assistprojectname,1&#x29; AS assistprojectname,   --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7684;&#x540d;&#x79f0;&#xd;&#xa;	a.year,&#xd;&#xa;	a.month,&#xd;&#xa;	case when CHARINDEX&#x28;&#x27;1&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_J &#xd;&#xa;     when CHARINDEX&#x28;&#x27;2&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_D  &#xd;&#xa;     ELSE a.period_fs end as sj&#xd;&#xa;from YW_YY_NC_ACCOUNTBALANCE a&#xd;&#xa;WHERE project_code IN &#x28;SELECT DISTINCT pk_corp as project_code FROM project WHERE PK_FATHERMEMBER&#x3d;&#x27;1001002&#x27;&#x29;&#xd;&#xa;and year&#x3d;year&#x28;DATEADD&#x28;month,-1, GETDATE&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from nc_km_sj where project_code&#x3d;&#x27;SG_BJ_ZJ_SY&#x27;and subject_code in  &#x28;&#x27;66010433&#x27;,&#x27;660301&#x27;,&#x27;660306&#x27;&#x29;&#xd;&#xa;,nc_km_last as&#x28;&#xd;&#xa;SELECT &#xd;&#xa;c.pk_corp as project_code,&#xd;&#xa;c.name as project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;&#xd;&#xa; c.year,&#xd;&#xa;	c.month,&#xd;&#xa;	sum&#x28;a.fx&#x2a; isnull&#x28;b.sj,0&#x29;&#x29; as  sj  --&#x672c;&#x671f;&#x53d1;&#x751f;  &#x501f;&#x65b9;&#xd;&#xa;from YW_HS_DIM_SUBJECT_KJKM_ZB a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN nc_km_sj b &#xd;&#xa;on replace&#x28;a.kj_code,&#x27;.&#x27;,&#x27;&#x27;&#x29;&#x3d;b.subject_code&#xd;&#xa;and a.assistprojecttype&#x3d;b.assistprojecttype&#xd;&#xa;and a.assistprojectname&#x3d;b.assistprojectname&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;and c.pk_corp&#x3d;b.project_code&#xd;&#xa;where a.source&#x3d;&#x27;km&#x27;&#xd;&#xa;and  c.PK_FATHERMEMBER&#x3d;&#x27;1001002&#x27;&#xd;&#xa;GROUP BY &#xd;&#xa;c.pk_corp,&#xd;&#xa;c.name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;--b.subject_code as kj_code,&#xd;&#xa;c.year,&#xd;&#xa;c.month&#xd;&#xa;&#x29; &#xd;&#xa;--SELECT &#x2a;from nc_km_last where  project_code&#x3d;&#x27;SG_BJ_ZJ_SY&#x27; and subject_code&#x3d;&#x27;DSYZ2.4.19&#x27;&#xd;&#xa;,nc_km_last_ok as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code,&#xd;&#xa;a.project_name,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;b.fullpath,&#xd;&#xa;a.year,&#xd;&#xa;a.month,&#xd;&#xa;a.sj&#xd;&#xa;from nc_km_last a&#xd;&#xa;INNER JOIN subject b &#xd;&#xa;on a.subject_code&#x3d;b.subject_code&#xd;&#xa;&#x29;&#xd;&#xa;--SELECT &#x2a; from nc_km_last_ok  where project_code&#x3d;&#x27;SG_BJ_ZJ_SY&#x27; and subject_code&#x3d;&#x27;DSYZ2.4.19&#x27;&#xd;&#xa;,nc_jbsj as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;c. pk_corp as project_code,&#xd;&#xa;c.name as project_name,&#xd;&#xa;a.level,&#xd;&#xa;a.IS_LAST,&#xd;&#xa;a.subject_code,&#xd;&#xa;a.subject_name,&#xd;&#xa;a.subject_px,&#xd;&#xa;c.year,&#xd;&#xa;c.month,&#xd;&#xa;b.sj&#xd;&#xa;from subject a&#xd;&#xa;LEFT JOIN project_shijian c&#xd;&#xa;on 1&#x3d;1&#xd;&#xa;LEFT JOIN nc_km_last_ok b&#xd;&#xa;on charindex&#x28;a.fullpath,b.fullpath&#x29;&#x3d;1&#xd;&#xa;and c.pk_corp&#x3d;b.project_code&#xd;&#xa;and c.year&#x3d;b.year&#xd;&#xa;and c.month&#x3d;b.month&#xd;&#xa;where   c.PK_FATHERMEMBER&#x3d;&#x27;1001002&#x27;&#xd;&#xa;&#x29;&#xd;&#xa;,zb_data as &#x28;&#xd;&#xa;SELECT &#x2a; from eas_jbsj &#xd;&#xa;union &#xd;&#xa;SELECT &#x2a; from nc_jbsj&#xd;&#xa;&#x29;&#xd;&#xa;--where project_code in &#x28;&#x27;SG_BJ_ZJ_SY&#x27;,&#x27;SG_HS_BJ&#x27;&#x29; and subject_code&#x3d;&#x27;DSYZ2.4.19&#x27; &#xd;&#xa;,fzz_sj as&#x28;&#xd;&#xa;&#xd;&#xa;SELECT &#xd;&#xa;  a.project_code,&#xd;&#xa;	a.project_name,&#xd;&#xa;	a.subject_code,&#xd;&#xa;	a.subject_name,&#xd;&#xa;	ISNULL&#x28;a.assistprojectnum,1&#x29; AS assistprojectnum,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;code&#xd;&#xa;	ISNULL&#x28;a.assistprojecttype,1&#x29; AS assistprojecttype,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.assistprojectname,1&#x29; AS assistprojectname ,  --&#x8f85;&#x52a9;&#x6838;&#x7b97;&#x7684;&#x540d;&#x79f0;&#xd;&#xa;  ISNULL&#x28;a.Fcontracttype_num,1&#x29; AS Fcontracttype_num ,--&#x8f85;&#x52a9;&#x5408;&#x540c;&#x7c7b;&#x578b;&#xd;&#xa;	ISNULL&#x28;a.Fcontracttype_name,1&#x29; AS Fcontracttype_name,--&#x8f85;&#x52a9;&#x5408;&#x540c;&#x7c7b;&#x578b;&#x540d;&#x79f0;&#xd;&#xa;  ISNULL&#x28;a.Fprojectperiod_name,1&#x29; AS Fprojectperiod_name,&#xd;&#xa;  ISNULL&#x28;a.Fprojectperiod_num,1&#x29; AS Fprojectperiod_num,&#xd;&#xa;	a.year,&#xd;&#xa;	a.month,&#xd;&#xa;	case when CHARINDEX&#x28;&#x27;1&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_J &#xd;&#xa;     when CHARINDEX&#x28;&#x27;2&#x27;,a.subject_code,1&#x29;&#x3d;1 then a.period_D  &#xd;&#xa;     ELSE a.period_fs end as sj&#xd;&#xa;from YW_YY_EAS_ASSISTBALANCE a&#xd;&#xa;WHERE a.project_code IN &#x28;SELECT DISTINCT pk_corp as project_code FROM project&#x29;&#xd;&#xa;&#x29;&#xd;&#xa;SELECT &#x2a; FROM &#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;--&#x27;SG_GS&#x27; as project_code,&#xd;&#xa;--&#x27;&#x5546;&#x7ba1;&#x603b;&#x90e8;&#x27; as project_name,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month,&#xd;&#xa;sum&#x28;isnull&#x28;sj,0&#x29;&#x29; as value,&#xd;&#xa;CONVERT&#x28;VARCHAR&#x28;100&#x29;,GETDATE&#x28;&#x29; ,120&#x29;as creat_date&#xd;&#xa;from   zb_data &#xd;&#xa;where  year&#x3d;year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;GROUP BY &#xd;&#xa;project_code,&#xd;&#xa;project_name,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;--project_code,&#xd;&#xa;--project_name,&#xd;&#xa;&#x27;SG_HSGS&#x27; as project_code,&#xd;&#xa;&#x27;&#x5408;&#x751f;&#x5546;&#x7ba1;&#x516c;&#x53f8;&#x27; as project_name,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month,&#xd;&#xa;sum&#x28;isnull&#x28;sj,0&#x29;&#x29; as value,&#xd;&#xa;CONVERT&#x28;VARCHAR&#x28;100&#x29;,GETDATE&#x28;&#x29; ,120&#x29;as creat_date&#xd;&#xa;from   zb_data &#xd;&#xa;where  year&#x3d;year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;AND PROJECT_CODE IN &#x28;SELECT DISTINCT PROJECT_CODE from YW_HS_SG_BI_MAP WHERE  PK_FATHERMEMBER&#x3d;&#x27;1001001&#x27; &#x29;&#xd;&#xa;GROUP BY subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;--project_code,&#xd;&#xa;--project_name,&#xd;&#xa;&#x27;SG_ZJGS&#x27; as project_code,&#xd;&#xa;&#x27;&#x73e0;&#x6c5f;&#x5546;&#x7ba1;&#x516c;&#x53f8;&#x27; as project_name,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month,&#xd;&#xa;sum&#x28;isnull&#x28;sj,0&#x29;&#x29; as value,&#xd;&#xa;CONVERT&#x28;VARCHAR&#x28;100&#x29;,GETDATE&#x28;&#x29; ,120&#x29;as creat_date&#xd;&#xa;from   zb_data &#xd;&#xa;where  year&#x3d;year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;AND PROJECT_CODE IN &#x28;SELECT DISTINCT PROJECT_CODE from YW_HS_SG_BI_MAP WHERE  PK_FATHERMEMBER&#x3d;&#x27;1001002&#x27; &#x29;&#xd;&#xa;GROUP BY subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;--project_code,&#xd;&#xa;--project_name,&#xd;&#xa;&#x27;SG_GS&#x27; as project_code,&#xd;&#xa;&#x27;&#x5546;&#x7ba1;&#x516c;&#x53f8;&#x27; as project_name,&#xd;&#xa;subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month,&#xd;&#xa;sum&#x28;isnull&#x28;sj,0&#x29;&#x29; as value,&#xd;&#xa;CONVERT&#x28;VARCHAR&#x28;100&#x29;,GETDATE&#x28;&#x29; ,120&#x29;as creat_date&#xd;&#xa;from   zb_data &#xd;&#xa;where  year&#x3d;year&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;and month&#x3d;month&#x28;dateadd&#x28;month,-1,getdate&#x28;&#x29;&#x29;&#x29;&#xd;&#xa;GROUP BY subject_code,&#xd;&#xa;subject_name, &#xd;&#xa;subject_px,&#xd;&#xa;year,month&#xd;&#xa;&#x29; A&#xd;&#xa;ORDER BY A.project_code,A.year,A.month,A.subject_px&#xd;&#xa;</sql>
