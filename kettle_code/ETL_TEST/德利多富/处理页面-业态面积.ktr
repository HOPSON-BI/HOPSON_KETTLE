    <sql>WITH ShopBase AS&#x28;&#xd;&#xa;SELECT&#xd;&#xa;a.project_code,--&#x9879;&#x76ee;&#x7f16;&#x7801;&#xd;&#xa;a.build_no,--&#x697c;&#x680b;&#xd;&#xa;a.floor,--&#x697c;&#x5c42;&#xd;&#xa;a.TradeName,--&#x94fa;&#x4f4d;&#x4e1a;&#x6001;&#xd;&#xa;a.berthid,--&#x94fa;&#x4f4d;&#x7f16;&#x53f7;&#xd;&#xa;a.BizDate,--&#x4e1a;&#x52a1;&#x65f6;&#x95f4;&#xd;&#xa;b.UnitID --&#x4e3b;&#x952e;&#xd;&#xa;FROM &#xd;&#xa;DW_DLDF_ShopBase a&#xd;&#xa;LEFT JOIN&#xd;&#xa;DW_DLDF_shopunit b &#xd;&#xa;ON a.ShopID&#x3d;b.ShopID AND a.BizDate&#x3d;b.BizDate&#xd;&#xa;WHERE a.ContractStatus&#x3d;&#x27;2&#x27; &#xd;&#xa;AND a.BizDate&#x3d;convert&#x28;varchar&#x28;10&#x29;,dateadd&#x28;day,-1,getdate&#x28;&#x29;&#x29;,23&#x29;&#xd;&#xa;&#x29;,--&#x5355;&#x5143;&#x72b6;&#x6001;&#x4e3a;2&#x7684;&#xd;&#xa;&#xd;&#xa;yic  as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;a.project_code ,--&#x9879;&#x76ee;&#x7f16;&#x7801;&#xd;&#xa;a.build_no,&#xd;&#xa;a.floor AS FloorName,--&#x697c;&#x5c42;&#xd;&#xa;b.TradeName as TradeName,--&#x5546;&#x94fa;&#x4e1a;&#x6001;&#xd;&#xa;a.BizDate,--&#x4e1a;&#x52a1;&#x65f6;&#x95f4;&#xd;&#xa;SUM&#x28;a.UserArea&#x29; AS UserArea --UserArea&#xd;&#xa;FROM DW_DLDF_Unit_Shop a&#xd;&#xa;left join ShopBase b ON a.UnitID&#x3d;b.UnitID &#xd;&#xa;WHERE convert&#x28;varchar&#x28;10&#x29;,a.Unit_start_time,23&#x29; &#x3c;&#x3d;convert&#x28;varchar&#x28;10&#x29;,a.BizDate,23&#x29; &#xd;&#xa;and isnull&#x28;a.Unit_end_time,&#x27;2200-12-31&#x27;&#x29;&#x3e;&#x3d;a.BizDate &#xd;&#xa;and a.BizDate &#x3d; convert&#x28;varchar&#x28;10&#x29;,dateadd&#x28;day,-1,getdate&#x28;&#x29;&#x29;,23&#x29;&#xd;&#xa;and a.UnitStatus&#x3c;&#x3e;&#x27;2&#x27;&#xd;&#xa;and left&#x28;a.UnitCode,2&#x29; &#x3c;&#x3e;&#x27;GG&#x27;&#xd;&#xa;and left&#x28;a.UnitCode,2&#x29; &#x3c;&#x3e;&#x27;CK&#x27;&#xd;&#xa;AND b.TradeName is not NULL&#xd;&#xa;GROUP BY a.project_code,a.build_no,a.floor,b.TradeName,a.BizDate&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;kb AS&#x28;&#xd;&#xa;SELECT &#xd;&#xa;project_code ,--&#x9879;&#x76ee;&#x7f16;&#x7801;&#xd;&#xa;build_no,&#xd;&#xa;floor,--&#x697c;&#x5c42;&#xd;&#xa;&#x27;&#x7a7a;&#x94fa;&#x27;  as TradeName,--&#x5546;&#x94fa;&#x4e1a;&#x6001;&#xd;&#xa;BizDate,--&#x4e1a;&#x52a1;&#x65f6;&#x95f4;&#xd;&#xa;SUM&#x28;UserArea&#x29; AS UserArea --UserArea&#xd;&#xa;FROM DW_DLDF_Unit_Shop &#xd;&#xa;WHERE convert&#x28;varchar&#x28;10&#x29;,Unit_start_time,23&#x29; &#x3c;&#x3d;convert&#x28;varchar&#x28;10&#x29;,BizDate,23&#x29; &#xd;&#xa;and isnull&#x28;Unit_end_time,&#x27;2200-12-31&#x27;&#x29;&#x3e;&#x3d;BizDate &#xd;&#xa;and BizDate &#x3d; convert&#x28;varchar&#x28;10&#x29;,dateadd&#x28;day,-1,getdate&#x28;&#x29;&#x29;,23&#x29;&#xd;&#xa;and UnitStatus&#x3d;&#x27;0&#x27;&#xd;&#xa;and left&#x28;UnitCode,2&#x29; &#x3c;&#x3e;&#x27;GG&#x27;&#xd;&#xa;and left&#x28;UnitCode,2&#x29; &#x3c;&#x3e;&#x27;CK&#x27;&#xd;&#xa;GROUP BY project_code,build_no,floor,BizDate&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;SELECT &#x2a; FROM yic&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#x2a; FROM kb</sql>
