    <connection>BI</connection>
    <sql>with orderkey AS &#x28;&#xd;&#xa;SELECT DISTINCT project_code,&#xd;&#xa;       project_name,&#xd;&#xa;       orderkey,&#xd;&#xa;			 convert&#x28;varchar&#x28;4&#x29;,getdate&#x28;&#x29;,120&#x29; as year&#xd;&#xa;FROM YW_HS_PROJECT_BASEINFO &#xd;&#xa;--WHERE branch &#x3d; &#x27;&#x5546;&#x4e1a;&#x4e8b;&#x4e1a;&#x90e8;&#x27;and project_code not in &#x28;&#x27;GZ_HSH_JH_NQ&#x27;,&#x27;GZ_HSH_JH_BQ&#x27;&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;rq as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;      DISTINCT&#xd;&#xa;      the_year year,&#xd;&#xa;			month&#x28;the_date&#x29; month&#xd;&#xa;FROM dim_time &#xd;&#xa;where the_year &#x3d; convert&#x28;varchar&#x28;4&#x29;,getdate&#x28;&#x29;,120&#x29;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;actual as &#x28;&#xd;&#xa;--&#x7ecf;&#x8425;&#x6536;&#x5165;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686410750&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_officebld_hotel_sr&#xd;&#xa;union all&#xd;&#xa;--EBITDA&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686463868&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_jsxm_comm_officebld_hotel_EBITDA&#xd;&#xa;union all&#xd;&#xa;--&#x9500;&#x552e;&#x989d;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686470474&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_sales&#xd;&#xa;WHERE project_code &#x3c;&#x3e; &#x27;SH_HSH_QPMGTD&#x27;&#xd;&#xa;union all&#xd;&#xa;SELECT &#xd;&#xa;      &#x27;&#x9500;&#x552e;&#x6307;&#x6807;&#x27; as index_type,&#xd;&#xa;			b.index_code,&#xd;&#xa;			b.project_code,&#xd;&#xa;			year&#x28;b.yearmonth &#x2b; &#x27;-01&#x27;&#x29; year,&#xd;&#xa;			month&#x28;b.yearmonth &#x2b; &#x27;-01&#x27;&#x29; month,&#xd;&#xa;			b.actual&#xd;&#xa;FROM YW_YY_JS_INDEX_SCORE_SX b&#xd;&#xa;WHERE exists &#x28;SELECT a.index_code FROM DIM_BI_BUS_RPT a WHERE a.index_code &#x3d; b.index_code&#x29;&#xd;&#xa;AND project_code &#x3d; &#x27;SH_HSH_QPMGTD&#x27;&#xd;&#xa;and index_code &#x3d; &#x27;BN76686470474&#x27;&#xd;&#xa;UNION ALL&#xd;&#xa;SELECT &#xd;&#xa;      &#x27;&#x9500;&#x552e;&#x6307;&#x6807;&#x27; as index_type,&#xd;&#xa;			b.index_code,&#xd;&#xa;			b.project_code,&#xd;&#xa;			year&#x28;b.yearmonth &#x2b; &#x27;-01&#x27;&#x29; year,&#xd;&#xa;			month&#x28;b.yearmonth &#x2b; &#x27;-01&#x27;&#x29; month,&#xd;&#xa;			b.actual&#xd;&#xa;FROM YW_YY_JS_INDEX_SCORE_JY b&#xd;&#xa;WHERE exists &#x28;SELECT a.index_code FROM DIM_BI_BUS_RPT a WHERE a.index_code &#x3d; b.index_code&#x29;&#xd;&#xa;AND project_code &#x3d; &#x27;SH_HSH_QPMGTD&#x27;&#xd;&#xa;and index_code &#x3d; &#x27;BN76686470474&#x27;&#xd;&#xa;AND yearmonth in &#x28;&#x27;2019-01&#x27;,&#x27;2019-02&#x27;&#x29;&#xd;&#xa;union all&#xd;&#xa;--&#x5ba2;&#x6d41;&#x91cf;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686465939&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_flow&#xd;&#xa;union all&#xd;&#xa;--&#x51fa;&#x79df;&#x7387;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686464181&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_officebld_chuzurate&#xd;&#xa;union all&#xd;&#xa;--&#x8d77;&#x79df;&#x7387;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686464869&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_officebld_qizurate&#xd;&#xa;union all&#xd;&#xa;--&#x6536;&#x7f34;&#x7387;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686463825&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_officeBld_sjl&#xd;&#xa;union all&#xd;&#xa;--&#x65b0;&#x7b7e;&#x62db;&#x5546;&#x9762;&#x79ef;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN76686471256&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_area_add&#xd;&#xa;union all&#xd;&#xa;--&#x7ecf;&#x8425;&#x6539;&#x9020;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN108834562487&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_jygz&#xd;&#xa;union all&#xd;&#xa;--&#x8f66;&#x6d41;&#x91cf;&#xd;&#xa;SELECT &#xd;&#xa;      index_type,&#xd;&#xa;			&#x27;BN108831143751&#x27; as index_code,&#xd;&#xa;			project_code,&#xd;&#xa;			year,&#xd;&#xa;			month,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_cll&#xd;&#xa;&#x2f;&#x2a;&#x9700;&#x8981;&#x4ece;&#x4e8b;&#x9879;&#x7c7b;&#x8868;&#x4e2d;&#x53d6;&#x7684;&#x6570;&#x636e;&#xd;&#xa;--&#x4e8b;&#x9879;&#x7c7b;&#x6570;&#x636e;&#xd;&#xa;SELECT &#xd;&#xa;      null as index_type,&#xd;&#xa;			b.index_code,&#xd;&#xa;			b.project_code,&#xd;&#xa;			year&#x28;b.yearmonth &#x2b; &#x27;-01&#x27;&#x29; year,&#xd;&#xa;			month&#x28;b.yearmonth &#x2b; &#x27;-01&#x27;&#x29; month,&#xd;&#xa;			b.actual&#xd;&#xa;FROM YW_YY_JS_INDEX_SCORE_SX b&#xd;&#xa;WHERE exists &#x28;SELECT a.index_code FROM DIM_BI_BUS_RPT a WHERE a.index_code &#x3d; b.index_code&#x29;&#xd;&#xa;AND project_code &#x3d; &#x27;BJ_HSH_WJQLS&#x27;&#xd;&#xa;&#x2a;&#x2f;&#xd;&#xa;&#x29;,&#xd;&#xa;&#xd;&#xa;budget as &#x28;&#xd;&#xa;SELECT &#xd;&#xa;      b.project_code,&#xd;&#xa;			b.yearmonth,&#xd;&#xa;			b.index_code,&#xd;&#xa;			b.budget,&#xd;&#xa;			b.actual&#xd;&#xa;FROM YW_YY_JS_INDEX_SCORE_JY b&#xd;&#xa;WHERE exists &#x28;SELECT a.index_code FROM DIM_BI_BUS_RPT a WHERE a.index_code &#x3d; b.index_code&#x29;&#xd;&#xa;--and index_code &#x3d; &#x27;BN76686470474&#x27;&#xd;&#xa;--and project_code &#x3d; &#x27;BJ_HSH_SDDJ&#x27;&#xd;&#xa;union all&#xd;&#xa;SELECT &#xd;&#xa;      b.project_code,&#xd;&#xa;			b.yearmonth,&#xd;&#xa;			b.index_code,&#xd;&#xa;			CASE isnumeric&#x28;budget&#x29; WHEN 1  THEN&#xd;&#xa;					CAST&#x28;budget as numeric&#x28;18,2&#x29;&#x29; ELSE null END budget,&#xd;&#xa;			CASE isnumeric&#x28;actual&#x29; WHEN 1  THEN&#xd;&#xa;					CAST&#x28;actual as numeric&#x28;18,2&#x29;&#x29; ELSE null END actual&#xd;&#xa;FROM YW_YY_JS_INDEX_SCORE_SX b&#xd;&#xa;WHERE exists &#x28;SELECT a.index_code FROM DIM_BI_BUS_RPT a WHERE a.index_code &#x3d; b.index_code&#x29;&#xd;&#xa;--and index_code &#x3d; &#x27;BN76686470474&#x27;&#xd;&#xa;--and project_code &#x3d; &#x27;BJ_HSH_SDDJ&#x27;&#xd;&#xa;union all&#xd;&#xa;--&#x7ecf;&#x8425;&#x6539;&#x9020;&#xd;&#xa;SELECT &#xd;&#xa;      project_code,&#xd;&#xa;			case when len&#x28;month&#x29; &#x3d; 1 then convert&#x28;varchar,year&#x29;&#x2b;&#x27;-0&#x27;&#x2b;convert&#x28;varchar,month&#x29;&#xd;&#xa;			     else convert&#x28;varchar,year&#x29;&#x2b;&#x27;-&#x27;&#x2b;convert&#x28;varchar,month&#x29; end as yearmonth,&#xd;&#xa;			&#x27;BN108834562487&#x27; as index_code,&#xd;&#xa;			budget,&#xd;&#xa;			actual &#xd;&#xa;FROM v_js_xm_comm_jygz&#xd;&#xa;&#x29;&#xd;&#xa;&#xd;&#xa;select &#xd;&#xa;      t5.project_code,&#xd;&#xa;			t1.index_code,&#xd;&#xa;			t1.index_name,&#xd;&#xa;			t2.year,&#xd;&#xa;			t2.month,&#xd;&#xa;      t3.budget,&#xd;&#xa;			t4.actual,&#xd;&#xa;			GETDATE&#x28;&#x29; as etl_time&#xd;&#xa;from orderkey t5&#xd;&#xa;left join DIM_BI_BUS_RPT t1 on t5.year &#x3d; t1.year&#xd;&#xa;left join rq t2 on t1.year &#x3d; t2.year&#xd;&#xa;left join budget t3 on t1.index_code &#x3d; t3.index_code and t2.year &#x3d; year&#x28;t3.yearmonth&#x2b;&#x27;-01&#x27;&#x29; and t2.month &#x3d; month&#x28;t3.yearmonth&#x2b;&#x27;-01&#x27;&#x29; and t3.project_code &#x3d; t5.project_code&#xd;&#xa;left join actual t4 on t1.index_code &#x3d; t4.index_code and t2.year &#x3d; t4.year and t2.month &#x3d; t4.month and t5.project_code &#x3d; t4.project_code&#xd;&#xa;--where t1.index_code &#x3d; &#x27;BN108834562487&#x27; and t5.project_code &#x3d; &#x27;BJ_HSH_SDDJ&#x27;&#xd;&#xa;ORDER BY t5.project_code,t1.order_id,t2.year,t2.month&#xd;&#xa;&#xd;&#xa;</sql>
