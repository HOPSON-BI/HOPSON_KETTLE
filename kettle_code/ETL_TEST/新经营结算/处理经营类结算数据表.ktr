    <connection>BI</connection>
    <sql>SELECT
a.project_code,
a.yearmonth,
a.index_code,
b.budget,
b.actual,
b.lj_budget,
b.lj_actual,
case when b.lj_budget=0 or len(b.lj_budget)=0 then null else b.lj_actual/b.lj_budget end as lj_rate
FROM YW_YY_JS_INDEX_SCORE_JY a
LEFT JOIN YW_HS_JS_MAP_INDEX c on a.project_code=c.project_code 
and a.yearmonth=c.yearmonth and a.index_code=c.index_code 
LEFT JOIN YW_YY_JS_INDEX_DATABASE b ON a.project_code=b.project_code 
and left(a.yearmonth,4)=b.year AND cast(right(a.yearmonth,2) as int)=b.month and c.map_index_type=b.index_type
WHERE a.index_code in ('BN76686410750','BN76686463868','BN76686475395','BN76686475512')
and a.yearmonth=convert(varchar(7),dateadd(month,-1,getdate()),23)
and day(getdate()) = (select date from yw_js_date)

UNION ALL

SELECT
a.project_code,
a.yearmonth,
a.index_code,
a.budget,
b.actual,
a.lj_budget,
b.lj_actual,
NULL
FROM YW_YY_JS_INDEX_SCORE_JY a
LEFT JOIN YW_HS_JS_MAP_INDEX c on a.project_code=c.project_code 
and a.yearmonth=c.yearmonth and a.index_code=c.index_code 
LEFT JOIN YW_YY_JS_INDEX_DATABASE b ON a.project_code=b.project_code 
and left(a.yearmonth,4)=b.year AND cast(right(a.yearmonth,2) as int)=b.month and c.map_index_type=b.index_type
WHERE a.index_code not in ('BN76686410750','BN76686463868','BN76686475395','BN76686475512')
and a.project_code &lt;&gt;'SH_HSH_QPMGTD'
and a.yearmonth=convert(varchar(7),dateadd(month,-1,getdate()),23)
and day(getdate()) = (select date from yw_js_date)</sql>
